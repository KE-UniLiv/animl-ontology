@prefix : <http://example.org/music-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:music-ontology a owl:Ontology .

# Classes
:Agent a owl:Class .
:Person a owl:Class ;
    rdfs:subClassOf :Agent .
:MusicArtist a owl:Class ;
    rdfs:subClassOf :Person .
:Publisher a owl:Class ;
    rdfs:subClassOf :Agent .

:Process a owl:Class .
:CreationProcess a owl:Class ;
    rdfs:subClassOf :Process .

:Place a owl:Class .
:Location a owl:Class ;
    rdfs:subClassOf :Place .

:Event a owl:Class .
:Award a owl:Class ;
    rdfs:subClassOf :Event .
:MusicalPerformance a owl:Class ;
    rdfs:subClassOf :Event .

:CreativeWork a owl:Class .
:MusicEntity a owl:Class ;
    rdfs:subClassOf :CreativeWork .

:Situation a owl:Class .
:PublicationSituation a owl:Class ;
    rdfs:subClassOf :Situation .

# Object Properties
:receivesNominationFor a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:takesPlaceAt a owl:ObjectProperty ;
    rdfs:domain :CreationProcess ;
    rdfs:range :Location .

:creates a owl:ObjectProperty ;
    rdfs:domain :MusicalPerformance ;
    rdfs:range :MusicEntity .

:involvesPublisher a owl:ObjectProperty ;
    rdfs:domain :PublicationSituation ;
    rdfs:range :Publisher .

# Restrictions
:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :receivesNominationFor ;
    owl:allValuesFrom :Award
] .

:CreationProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :takesPlaceAt ;
    owl:allValuesFrom :Location
] .
:CreationProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :takesPlaceAt ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:MusicalPerformance rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :creates ;
    owl:allValuesFrom :MusicEntity
] .
:MusicalPerformance rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :creates ;
    owl:someValuesFrom :MusicEntity
] .

:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :involvesPublisher ;
    owl:allValuesFrom :Publisher
] .
:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :involvesPublisher ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

# Disjoint Classes
:Agent owl:disjointWith :Process .
:Agent owl:disjointWith :Place .
:Agent owl:disjointWith :Event .
:Agent owl:disjointWith :CreativeWork .
:Agent owl:disjointWith :Situation .

:Process owl:disjointWith :Place .
:Process owl:disjointWith :Event .
:Process owl:disjointWith :CreativeWork .
:Process owl:disjointWith :Situation .

:Place owl:disjointWith :Event .
:Place owl:disjointWith :CreativeWork .
:Place owl:disjointWith :Situation .

:Event owl:disjointWith :CreativeWork .
:Event owl:disjointWith :Situation .

:CreativeWork owl:disjointWith :Situation .