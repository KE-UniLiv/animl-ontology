@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/music-ontology#> .

ex:MusicOntology a owl:Ontology .

# Classes
ex:Agent a owl:Class .
ex:Artist a owl:Class ;
    rdfs:subClassOf ex:Agent .
ex:MusicArtist a owl:Class ;
    rdfs:subClassOf ex:Artist .

ex:Award a owl:Class .

ex:CreativeWork a owl:Class .
ex:Recording a owl:Class ;
    rdfs:subClassOf ex:CreativeWork .
ex:MusicalWork a owl:Class ;
    rdfs:subClassOf ex:CreativeWork .
ex:Score a owl:Class ;
    rdfs:subClassOf ex:CreativeWork .
ex:AbstractScore a owl:Class ;
    rdfs:subClassOf ex:Score .
ex:Text a owl:Class ;
    rdfs:subClassOf ex:CreativeWork .
ex:Statement a owl:Class ;
    rdfs:subClassOf ex:Text .
ex:DedicationStatement a owl:Class ;
    rdfs:subClassOf ex:Statement .
ex:Release a owl:Class ;
    rdfs:subClassOf ex:CreativeWork .

ex:Process a owl:Class .
ex:RecordingProcess a owl:Class ;
    rdfs:subClassOf ex:Process .
ex:CreativeProcess a owl:Class ;
    rdfs:subClassOf ex:Process .

ex:Equipment a owl:Class .

ex:Event a owl:Class .
ex:Performance a owl:Class ;
    rdfs:subClassOf ex:Event .
ex:MusicalPerformance a owl:Class ;
    rdfs:subClassOf ex:Performance .
ex:PublicationSituation a owl:Class ;
    rdfs:subClassOf ex:Event .
ex:BroadcastingSituation a owl:Class ;
    rdfs:subClassOf ex:Event .

ex:Person a owl:Class ;
    rdfs:subClassOf ex:Agent .

ex:FormType a owl:Class .

ex:Genre a owl:Class .
ex:MusicGenre a owl:Class ;
    rdfs:subClassOf ex:Genre .

ex:Instrumentation a owl:Class .

ex:MusicalConcept a owl:Class .
ex:MusicalKey a owl:Class ;
    rdfs:subClassOf ex:MusicalConcept .

ex:LegalDocument a owl:Class .
ex:License a owl:Class ;
    rdfs:subClassOf ex:LegalDocument .

ex:Organization a owl:Class ;
    rdfs:subClassOf ex:Agent .
ex:Publisher a owl:Class ;
    rdfs:subClassOf ex:Organization .
ex:Broadcaster a owl:Class ;
    rdfs:subClassOf ex:Organization .
ex:MusicEnsemble a owl:Class ;
    rdfs:subClassOf ex:Organization .

ex:Place a owl:Class .
ex:Location a owl:Class ;
    rdfs:subClassOf ex:Place .

ex:Membership a owl:Class .
ex:TimeInterval a owl:Class .

# Object Properties
ex:receivesNominationFor a owl:ObjectProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:Award .
ex:isNominatedFor a owl:ObjectProperty ;
    owl:inverseOf ex:receivesNominationFor .

ex:receivesAward a owl:ObjectProperty ;
    rdfs:subPropertyOf ex:receivesNominationFor ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:Award .
ex:isReceivedBy a owl:ObjectProperty ;
    owl:inverseOf ex:receivesAward .

ex:produces a owl:ObjectProperty ;
    rdfs:domain ex:RecordingProcess ;
    rdfs:range ex:Recording .
ex:isProducedBy a owl:ObjectProperty ;
    owl:inverseOf ex:produces .

ex:involves a owl:ObjectProperty ;
    rdfs:domain ex:RecordingProcess ;
    rdfs:range ex:Equipment .
ex:isUsedIn a owl:ObjectProperty ;
    owl:inverseOf ex:involves .

ex:records a owl:ObjectProperty ;
    rdfs:domain ex:RecordingProcess ;
    rdfs:range ex:MusicalPerformance .
ex:isRecordedBy a owl:ObjectProperty ;
    owl:inverseOf ex:records .

ex:involvesAgent a owl:ObjectProperty ;
    rdfs:domain ex:CreativeProcess ;
    rdfs:range ex:Agent .
ex:isAgentIn a owl:ObjectProperty ;
    owl:inverseOf ex:involvesAgent .

ex:creates a owl:ObjectProperty ;
    rdfs:domain ex:Process ;
    rdfs:range ex:MusicalWork .
ex:isCreatedBy a owl:ObjectProperty ;
    owl:inverseOf ex:creates .

ex:createsRecording a owl:ObjectProperty ;
    rdfs:domain ex:MusicalPerformance ;
    rdfs:range ex:Recording .
ex:isCreatedDuring a owl:ObjectProperty ;
    owl:inverseOf ex:createsRecording .

ex:isDedicatedTo a owl:ObjectProperty ;
    rdfs:domain ex:DedicationStatement ;
    rdfs:range ex:Person .
ex:dedicates a owl:ObjectProperty ;
    owl:inverseOf ex:isDedicatedTo .

ex:hasFormType a owl:ObjectProperty ;
    rdfs:domain ex:AbstractScore ;
    rdfs:range ex:FormType .
ex:isFormTypeOf a owl:ObjectProperty ;
    owl:inverseOf ex:hasFormType .

ex:hasMusicGenre a owl:ObjectProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:MusicGenre .
ex:isGenreOf a owl:ObjectProperty ;
    owl:inverseOf ex:hasMusicGenre .

ex:hasInstrumentation a owl:ObjectProperty ;
    rdfs:domain ex:AbstractScore ;
    rdfs:range ex:Instrumentation .
ex:isInstrumentationOf a owl:ObjectProperty ;
    owl:inverseOf ex:hasInstrumentation .

ex:isInKey a owl:ObjectProperty ;
    rdfs:domain ex:AbstractScore ;
    rdfs:range ex:MusicalKey .
ex:isKeyOf a owl:ObjectProperty ;
    owl:inverseOf ex:isInKey .

ex:hasLicense a owl:ObjectProperty ;
    rdfs:domain ex:CreativeWork ;
    rdfs:range ex:License .
ex:isLicensedBy a owl:ObjectProperty ;
    owl:inverseOf ex:hasLicense .

ex:involvesPublisher a owl:ObjectProperty ;
    rdfs:domain ex:PublicationSituation ;
    rdfs:range ex:Publisher .
ex:isPublisherIn a owl:ObjectProperty ;
    owl:inverseOf ex:involvesPublisher .

ex:hasPublicationLocation a owl:ObjectProperty ;
    rdfs:domain ex:PublicationSituation ;
    rdfs:range ex:Location .
ex:isLocationOfPublication a owl:ObjectProperty ;
    owl:inverseOf ex:hasPublicationLocation .

ex:isRecordingOf a owl:ObjectProperty ;
    rdfs:domain ex:Recording ;
    rdfs:range ex:MusicalWork .
ex:hasRecording a owl:ObjectProperty ;
    owl:inverseOf ex:isRecordingOf .

ex:isSourceOf a owl:ObjectProperty ;
    rdfs:domain ex:Text ;
    rdfs:range ex:Text .
ex:hasSource a owl:ObjectProperty ;
    owl:inverseOf ex:isSourceOf .

ex:hasAssociatedText a owl:ObjectProperty ;
    rdfs:domain ex:AbstractScore ;
    rdfs:range ex:Text .
ex:isAssociatedWithScore a owl:ObjectProperty ;
    owl:inverseOf ex:hasAssociatedText .

ex:involvesBroadcaster a owl:ObjectProperty ;
    rdfs:domain ex:BroadcastingSituation ;
    rdfs:range ex:Broadcaster .
ex:isBroadcasterIn a owl:ObjectProperty ;
    owl:inverseOf ex:involvesBroadcaster .

ex:wasFormedAt a owl:ObjectProperty ;
    rdfs:domain ex:MusicEnsemble ;
    rdfs:range ex:Location .
ex:formedEnsembleAt a owl:ObjectProperty ;
    owl:inverseOf ex:wasFormedAt .

ex:collaboratedWith a owl:ObjectProperty ;
    rdf:type owl:SymmetricProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:MusicArtist .

ex:wasInfluencedBy a owl:ObjectProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:MusicArtist .
ex:influenced a owl:ObjectProperty ;
    owl:inverseOf ex:wasInfluencedBy .

ex:hasMembership a owl:ObjectProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range ex:Membership .
ex:isMembershipOfArtist a owl:ObjectProperty ;
