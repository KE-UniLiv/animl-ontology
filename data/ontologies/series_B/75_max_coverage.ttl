    rdfs:range ex:MusicEnsemble .
ex:hasMembershipForEnsemble a owl:ObjectProperty ;
    owl:inverseOf ex:isMembershipOfEnsemble .

ex:hasMember a owl:ObjectProperty ;
    rdfs:domain ex:MusicEnsemble ;
    rdfs:range ex:MusicArtist .
ex:isMemberOf a owl:ObjectProperty ;
    owl:inverseOf ex:hasMember .

ex:hasTimeInterval a owl:ObjectProperty ;
    rdfs:domain ex:Membership ;
    rdfs:range ex:TimeInterval .
ex:isTimeIntervalOf a owl:ObjectProperty ;
    owl:inverseOf ex:hasTimeInterval .

# Datatype Properties
ex:hasPublicationDate a owl:DatatypeProperty ;
    rdfs:domain ex:PublicationSituation ;
    rdfs:range xsd:date .

ex:hasBroadcastingDate a owl:DatatypeProperty ;
    rdfs:domain ex:BroadcastingSituation ;
    rdfs:range xsd:date .

ex:hasActivityStartDate a owl:DatatypeProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range xsd:date .

ex:hasActivityEndDate a owl:DatatypeProperty ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range xsd:date .

ex:hasStartDate a owl:DatatypeProperty ;
    rdfs:domain ex:TimeInterval ;
    rdfs:range xsd:date .

ex:hasEndDate a owl:DatatypeProperty ;
    rdfs:domain ex:TimeInterval ;
    rdfs:range xsd:date .

# Disjoint Classes
ex:MusicArtist owl:disjointWith ex:Award .
ex:Recording owl:disjointWith ex:RecordingProcess .
ex:Equipment owl:disjointWith ex:RecordingProcess .
ex:RecordingProcess owl:disjointWith ex:MusicalPerformance .
ex:Agent owl:disjointWith ex:CreativeProcess .
ex:Process owl:disjointWith ex:MusicalWork .
ex:MusicalWork owl:disjointWith ex:MusicalPerformance .
ex:DedicationStatement owl:disjointWith ex:Person .
ex:AbstractScore owl:disjointWith ex:FormType .
ex:MusicArtist owl:disjointWith ex:MusicGenre .
ex:AbstractScore owl:disjointWith ex:Instrumentation .
ex:AbstractScore owl:disjointWith ex:MusicalKey .
ex:Release owl:disjointWith ex:License .
ex:Score owl:disjointWith ex:License .
ex:Publisher owl:disjointWith ex:PublicationSituation .
ex:PublicationSituation owl:disjointWith ex:Location .
ex:Recording owl:disjointWith ex:MusicalWork .
ex:AbstractScore owl:disjointWith ex:Text .
ex:Broadcaster owl:disjointWith ex:BroadcastingSituation .
ex:MusicEnsemble owl:disjointWith ex:Location .
