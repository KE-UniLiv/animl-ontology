@prefix : <http://example.org/music-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:musicOntology a owl:Ontology .

# Classes
:Agent a owl:Class .
:MusicArtist a owl:Class ;
    rdfs:subClassOf :Agent .
:MusicEnsemble a owl:Class ;
    rdfs:subClassOf :Agent .
:Publisher a owl:Class ;
    rdfs:subClassOf :Agent .
:Broadcaster a owl:Class ;
    rdfs:subClassOf :Agent .

:Process a owl:Class .
:RecordingProcess a owl:Class ;
    rdfs:subClassOf :Process .
:CreationProcess a owl:Class ;
    rdfs:subClassOf :Process .
:CreativeProcess a owl:Class ;
    rdfs:subClassOf :Process .
:PublicationSituation a owl:Class ;
    rdfs:subClassOf :Process .
:BroadcastingSituation a owl:Class ;
    rdfs:subClassOf :Process .
:MusicalPerformance a owl:Class ;
    rdfs:subClassOf :Process .

:Entity a owl:Class .
:MusicEntity a owl:Class ;
    rdfs:subClassOf :Entity .
:CreativeWork a owl:Class ;
    rdfs:subClassOf :MusicEntity .
:AbstractScore a owl:Class ;
    rdfs:subClassOf :CreativeWork .
:Text a owl:Class ;
    rdfs:subClassOf :CreativeWork .
:Product a owl:Class ;
    rdfs:subClassOf :MusicEntity .
:Recording a owl:Class ;
    rdfs:subClassOf :Product .
:Release a owl:Class ;
    rdfs:subClassOf :Product .
:Award a owl:Class ;
    rdfs:subClassOf :Entity .
:CreativeAction a owl:Class ;
    rdfs:subClassOf :Entity .
:Membership a owl:Class ;
    rdfs:subClassOf :Entity .

# Object Properties
:hasReceivedNominationFor a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:hasReceived a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:produces a owl:ObjectProperty ;
    rdfs:domain :RecordingProcess ;
    rdfs:range :Recording .

:recorded a owl:ObjectProperty ;
    rdfs:domain :RecordingProcess ;
    rdfs:range :MusicalPerformance .

:hasCreativeAction a owl:ObjectProperty ;
    rdfs:domain :CreativeProcess ;
    rdfs:range :CreativeAction .

:creates a owl:ObjectProperty ;
    rdfs:domain :MusicalPerformance ;
    rdfs:range :MusicEntity .

:involvesPublisher a owl:ObjectProperty ;
    rdfs:domain :PublicationSituation ;
    rdfs:range :Publisher .

:isSourceOf a owl:ObjectProperty ;
    rdfs:domain :Text ;
    rdfs:range :Text .

:involvesBroadcaster a owl:ObjectProperty ;
    rdfs:domain :BroadcastingSituation ;
    rdfs:range :Broadcaster .

:wasInfluencedBy a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtist .

:hasMember a owl:ObjectProperty ;
    rdfs:domain :Membership ;
    rdfs:range :MusicArtist .

:isMembershipOf a owl:ObjectProperty ;
    rdfs:domain :Membership ;
    rdfs:range :MusicEnsemble .

# Datatype Properties
:hasTimeInterval a owl:DatatypeProperty ;
    rdfs:domain :CreationProcess ;
    rdfs:range xsd:dateTime .

:hasFormType a owl:DatatypeProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range xsd:string .

:hasInstrumentation a owl:DatatypeProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range xsd:string .

:hasLicense a owl:DatatypeProperty ;
    rdfs:domain :Release ;
    rdfs:range xsd:string .

:happenedAtLocation a owl:DatatypeProperty ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf (:PublicationSituation :BroadcastingSituation)
    ] ;
    rdfs:range xsd:string .

:wasFormedAtLocation a owl:DatatypeProperty ;
    rdfs:domain :MusicEnsemble ;
    rdfs:range xsd:string .

:hasPeriod a owl:DatatypeProperty ;
    rdfs:domain :Membership ;
    rdfs:range xsd:string .

:hasActivityStartDate a owl:DatatypeProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range xsd:date .

:hasName a owl:DatatypeProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range xsd:string .

# Disjoint Classes
:MusicArtist owl:disjointWith :MusicEnsemble .
:Publisher owl:disjointWith :Broadcaster .
:Recording owl:disjointWith :AbstractScore .
:CreativeWork owl:disjointWith :Product .
:Agent owl:disjointWith :Process , :Entity .
:Process owl:disjointWith :Entity .

# Restrictions (Cardinality)
:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasName ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasCreativeAction ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :involvesPublisher ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:BroadcastingSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :involvesBroadcaster ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:RecordingProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :produces ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

:Membership rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasMember ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
] .

:Membership rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :isMembershipOf ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
] .