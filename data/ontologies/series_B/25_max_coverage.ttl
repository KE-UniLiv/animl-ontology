@prefix : <http://example.org/music-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/music-ontology> rdf:type owl:Ontology .

# Top-level Classes
:Person rdf:type owl:Class .
:Recognition rdf:type owl:Class .
:Process rdf:type owl:Class .
:Place rdf:type owl:Class .
:Performance rdf:type owl:Class .
:Entity rdf:type owl:Class .
:Event rdf:type owl:Class .
:Organization rdf:type owl:Class .

# Subclasses
:MusicArtist rdf:type owl:Class ;
    rdfs:subClassOf :Person .

:Award rdf:type owl:Class ;
    rdfs:subClassOf :Recognition .

:CreationProcess rdf:type owl:Class ;
    rdfs:subClassOf :Process .

:Location rdf:type owl:Class ;
    rdfs:subClassOf :Place .

:MusicalPerformance rdf:type owl:Class ;
    rdfs:subClassOf :Performance .

:MusicEntity rdf:type owl:Class ;
    rdfs:subClassOf :Entity .
:Score rdf:type owl:Class ;
    rdfs:subClassOf :MusicEntity .
:AbstractScore rdf:type owl:Class ;
    rdfs:subClassOf :Score .

:PublicationSituation rdf:type owl:Class ;
    rdfs:subClassOf :Event .

:Publisher rdf:type owl:Class ;
    rdfs:subClassOf :Organization .
:Ensemble rdf:type owl:Class ;
    rdfs:subClassOf :Organization .
:MusicEnsemble rdf:type owl:Class ;
    rdfs:subClassOf :Ensemble .

# Disjoint Classes (Top-level pairwise disjointness)
:Person owl:disjointWith :Recognition , :Process , :Place , :Performance , :Entity , :Event , :Organization .
:Recognition owl:disjointWith :Process , :Place , :Performance , :Entity , :Event , :Organization .
:Process owl:disjointWith :Place , :Performance , :Entity , :Event , :Organization .
:Place owl:disjointWith :Performance , :Entity , :Event , :Organization .
:Performance owl:disjointWith :Entity , :Event , :Organization .
:Entity owl:disjointWith :Event , :Organization .
:Event owl:disjointWith :Organization .

# Object Properties
:isNominatedFor rdf:type owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:receivedAward rdf:type owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:hasLocation rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ; owl:unionOf (:CreationProcess :PublicationSituation) ] ;
    rdfs:range :Location .

:creates rdf:type owl:ObjectProperty ;
    rdfs:domain :MusicalPerformance ;
    rdfs:range :MusicEntity .

:involvesPublisher rdf:type owl:ObjectProperty ;
    rdfs:domain :PublicationSituation ;
    rdfs:range :Publisher .

:wasFormedAt rdf:type owl:ObjectProperty ;
    rdfs:domain :MusicEnsemble ;
    rdfs:range :Location .

:wasInfluencedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtist .

# Datatype Properties
:hasFormType rdf:type owl:DatatypeProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range xsd:string .

:hasTime rdf:type owl:DatatypeProperty ;
    rdfs:domain :PublicationSituation ;
    rdfs:range xsd:date .

# Restrictions (Cardinality)

# CQ1: Which award has a music artist received a nomination for?
:MusicArtist rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :isNominatedFor ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .
:Award rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :isNominatedFor ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ2: Which award has a music artist received?
:MusicArtist rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :receivedAward ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .
:Award rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :receivedAward ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ3: Where did the creation process took place?
:CreationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :hasLocation ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

# CQ4: Which music entity is created during a musical performance?
:MusicalPerformance rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :creates ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .
:MusicEntity rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :creates ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ5: Which is the form type of an abstract score?
:AbstractScore rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :hasFormType ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ6: Which is the publisher involved in a publication situation?
:PublicationSituation rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :involvesPublisher ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .
:Publisher rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :involvesPublisher ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ7: When did a publication situation happen?
:PublicationSituation rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :hasTime ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

# CQ8: Where did a publication situation happen?
:PublicationSituation rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :hasLocation ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .

# CQ9: Where a music ensemble was formed?
:MusicEnsemble rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :wasFormedAt ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
] .
:Location rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :wasFormedAt ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# CQ10: Which music artist was a music artist influenced by?
:MusicArtist rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :wasInfluencedBy ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .
:MusicArtist rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :wasInfluencedBy ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .

# General inverse restriction for hasLocation (since its domain is a union)
:Location rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty [ owl:inverseOf :hasLocation ] ;
    owl:minCardinality "0"^^xsd:nonNegativeInteger
] .