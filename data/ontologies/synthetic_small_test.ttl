@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/music-ontology#> .

:music-ontology a owl:Ontology .

# Classes
:Agent a owl:Class .
:Person rdfs:subClassOf :Agent .
:MusicArtist rdfs:subClassOf :Person .
:MusicEnsemble rdfs:subClassOf :Agent .
:Publisher rdfs:subClassOf :Agent .
:Broadcaster rdfs:subClassOf :Agent .

:Event a owl:Class .
:Award rdfs:subClassOf :Event .
:Performance rdfs:subClassOf :Event .
:MusicalPerformance rdfs:subClassOf :Performance .
:Process rdfs:subClassOf :Event .
:RecordingProcess rdfs:subClassOf :Process .
:CreativeProcess rdfs:subClassOf :Process .
:Situation rdfs:subClassOf :Event .
:PublicationSituation rdfs:subClassOf :Situation .
:BroadcastingSituation rdfs:subClassOf :Situation .

:MusicEntity a owl:Class .
:Recording rdfs:subClassOf :MusicEntity .
:MusicalPiece rdfs:subClassOf :MusicEntity .
:Score rdfs:subClassOf :MusicEntity .
:AbstractScore rdfs:subClassOf :Score .
:Release rdfs:subClassOf :MusicEntity .

:InformationContentEntity a owl:Class .
:Text rdfs:subClassOf :InformationContentEntity .
:DedicationStatement rdfs:subClassOf :InformationContentEntity .
:OpusStatement rdfs:subClassOf :InformationContentEntity .
:Label rdfs:subClassOf :InformationContentEntity .
:MusicArtistName rdfs:subClassOf :Label .
:MusicArtistAlias rdfs:subClassOf :Label .

:Concept a owl:Class .
:FormType rdfs:subClassOf :Concept .
:MusicGenre rdfs:subClassOf :Concept .
:Instrumentation rdfs:subClassOf :Concept .
:MusicalKey rdfs:subClassOf :Concept .

:LegalDocument a owl:Class .
:License rdfs:subClassOf :LegalDocument .

:PhysicalObject a owl:Class .
:Equipment rdfs:subClassOf :PhysicalObject .

:Algorithm a owl:Class .
:MusicAlgorithm rdfs:subClassOf :Algorithm .

:Dataset a owl:Class .
:MusicDataset rdfs:subClassOf :Dataset .

:Action a owl:Class .
:CreativeAction rdfs:subClassOf :Action .

:Relationship a owl:Class .
:EnsembleMembership rdfs:subClassOf :Relationship .

# Object Properties
:hasNominationFor a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:hasReceived a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :Award .

:isProducedDuring a owl:ObjectProperty ;
    rdfs:domain :Recording ;
    rdfs:range :RecordingProcess .

:usesEquipment a owl:ObjectProperty ;
    rdfs:domain :RecordingProcess ;
    rdfs:range :Equipment .

:recordsPerformance a owl:ObjectProperty ;
    rdfs:domain :RecordingProcess ;
    rdfs:range :MusicalPerformance .

:hasParticipant a owl:ObjectProperty ;
    rdfs:domain :CreativeProcess ;
    rdfs:range :Agent .

:hasCreativeAction a owl:ObjectProperty ;
    rdfs:domain :CreativeProcess ;
    rdfs:range :CreativeAction .

:creates a owl:ObjectProperty ;
    rdfs:domain :Process ;
    rdfs:range :MusicEntity .

:isDedicatedTo a owl:ObjectProperty ;
    rdfs:domain :DedicationStatement ;
    rdfs:range :Person .

:hasFormType a owl:ObjectProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range :FormType .

:hasGenre a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicGenre .

:hasInstrumentation a owl:ObjectProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range :Instrumentation .

:hasKey a owl:ObjectProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range :MusicalKey .

:hasLicense a owl:ObjectProperty ;
    rdfs:domain :MusicEntity ;
    rdfs:range :License .

:hasOpusStatement a owl:ObjectProperty ;
    rdfs:domain :MusicEntity ;
    rdfs:range :OpusStatement .

:hasPublisher a owl:ObjectProperty ;
    rdfs:domain :PublicationSituation ;
    rdfs:range :Publisher .

:isRecordingOf a owl:ObjectProperty ;
    rdfs:domain :Recording ;
    rdfs:range :MusicEntity .

:hasRecording a owl:ObjectProperty ;
    rdfs:domain :MusicEntity ;
    rdfs:range :Recording ;
    owl:inverseOf :isRecordingOf .

:hasSourceText a owl:ObjectProperty ;
    rdfs:domain :Text ;
    rdfs:range :Text .

:hasAssociatedText a owl:ObjectProperty ;
    rdfs:domain :AbstractScore ;
    rdfs:range :Text .

:hasBroadcaster a owl:ObjectProperty ;
    rdfs:domain :BroadcastingSituation ;
    rdfs:range :Broadcaster .

:trainedOn a owl:ObjectProperty ;
    rdfs:domain :MusicAlgorithm ;
    rdfs:range :MusicDataset .

:collaboratedWith a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtist .

:influencedBy a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtist .

:hasMembership a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :EnsembleMembership .

:isMembershipOf a owl:ObjectProperty ;
    rdfs:domain :EnsembleMembership ;
    rdfs:range :MusicEnsemble .

:hasMember a owl:ObjectProperty ;
    rdfs:domain :MusicEnsemble ;
    rdfs:range :MusicArtist .

:hasNameEntity a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtistName .

:hasAliasEntity a owl:ObjectProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range :MusicArtistAlias .

:hasPart a owl:ObjectProperty ;
    rdfs:domain :MusicEntity ;
    rdfs:range :MusicEntity .

# Datatype Properties
:hasTime a owl:DatatypeProperty ;
    rdfs:domain :Event ;
    rdfs:range xsd:dateTime .

:hasLocation a owl:DatatypeProperty ;
    rdfs:domain :Event ;
    rdfs:range xsd:string .

:hasFormationLocation a owl:DatatypeProperty ;
    rdfs:domain :MusicEnsemble ;
    rdfs:range xsd:string .

:hasRole a owl:DatatypeProperty ;
    rdfs:domain :EnsembleMembership ;
    rdfs:range xsd:string .

:hasTimeInterval a owl:DatatypeProperty ;
    rdfs:domain :EnsembleMembership ;
    rdfs:range xsd:string .

:hasStartDate a owl:DatatypeProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range xsd:date .

:hasEndDate a owl:DatatypeProperty ;
    rdfs:domain :MusicArtist ;
    rdfs:range xsd:date .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :Label ;
    rdfs:range xsd:string .

:hasLanguage a owl:DatatypeProperty ;
    rdfs:domain :Label ;
    rdfs:range xsd:string .

# Restrictions
:Recording rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :isProducedDuring ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :RecordingProcess
] .

:RecordingProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :usesEquipment ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :Equipment
] .

:RecordingProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :recordsPerformance ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :MusicalPerformance
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasParticipant ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :Agent
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasCreativeAction ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :CreativeAction
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :creates ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :MusicalPiece
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasTime ;
    owl:minCardinality 0
] .

:CreativeProcess rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLocation ;
    owl:minCardinality 0
] .

:MusicalPerformance rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :creates ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicEntity
] .

:DedicationStatement rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :isDedicatedTo ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :Person
] .

:AbstractScore rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasFormType ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :FormType
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasGenre ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicGenre
] .

:AbstractScore rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasInstrumentation ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :Instrumentation
] .

:AbstractScore rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasKey ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicalKey
] .

:Release rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLicense ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :License
] .

:Score rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLicense ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :License
] .

:Recording rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLicense ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :License
] .

:MusicEntity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasOpusStatement ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :OpusStatement
] .

:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasPublisher ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :Publisher
] .

:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasTime ;
    owl:minCardinality 0
] .

:PublicationSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLocation ;
    owl:minCardinality 0
] .

:MusicEntity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasRecording ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :Recording
] .

:Recording rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :isRecordingOf ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :MusicEntity
] .

:Text rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasSourceText ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :Text
] .

:AbstractScore rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasAssociatedText ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :Text
] .

:BroadcastingSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasBroadcaster ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :Broadcaster
] .

:BroadcastingSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasTime ;
    owl:minCardinality 0
] .

:BroadcastingSituation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLocation ;
    owl:minCardinality 0
] .

:MusicAlgorithm rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :trainedOn ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicDataset
] .

:MusicEnsemble rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasFormationLocation ;
    owl:minCardinality 0
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :collaboratedWith ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicArtist
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :influencedBy ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicArtist
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasMembership ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :EnsembleMembership
] .

:EnsembleMembership rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :isMembershipOf ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :MusicEnsemble
] .

:EnsembleMembership rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasRole ;
    owl:minCardinality 0
] .

:EnsembleMembership rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasTimeInterval ;
    owl:minCardinality 0
] .

:MusicEnsemble rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasMember ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicArtist
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasStartDate ;
    owl:minCardinality 0
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasEndDate ;
    owl:minCardinality 0
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasNameEntity ;
    owl:minCardinality 1 ;
    owl:someValuesFrom :MusicArtistName
] .

:MusicArtist rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasAliasEntity ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicArtistAlias
] .

:Label rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasValue ;
    owl:minCardinality 1
] .

:Label rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasLanguage ;
    owl:minCardinality 0
] .

:MusicEntity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasPart ;
    owl:minCardinality 0 ;
    owl:someValuesFrom :MusicEntity
] .

# Disjoint Classes
:Person owl:disjointWith :MusicEnsemble .
:Process owl:disjointWith :Situation .
:MusicEntity owl:disjointWith :Agent .
:MusicEntity owl:disjointWith :Event .
:MusicEntity owl:disjointWith :PhysicalObject .
:Recording owl:disjointWith :Score .
:Recording owl:disjointWith :MusicalPiece .
:Score owl:disjointWith :MusicalPiece .
:Release owl:disjointWith :Recording .
:Release owl:disjointWith :Score .
:Release owl:disjointWith :MusicalPiece .
