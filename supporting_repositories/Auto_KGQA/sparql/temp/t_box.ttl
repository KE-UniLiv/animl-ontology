@prefix kgqa: <http://www.kgqa.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/music-ontology#RecordingProcessThatRecordedMusicalPerformance> a owl:Class ;
    rdfs:label "music-ontology Recording Process That Recorded Musical Performance" ;
    kgqa:count 1 ;
    rdfs:comment "A specific type of recording process that captured a musical performance." ;
    rdfs:subClassOf <file:///C:/Users/sgwmorli/internship_stage_2/animl_ontology_2/superclass>,
        <http://example.org/music-ontology#RecordingProcess> .

<http://example.org/music-ontology#creates> a rdf:Property ;
    rdfs:label "music-ontology creates" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a creative process to the musical piece it creates." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range <http://example.org/music-ontology#MusicalPiece> .

<http://example.org/music-ontology#hasCreativeAction> a rdf:Property ;
    rdfs:label "music-ontology has Creative Action" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a creative process to its constituent creative actions." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range <http://example.org/music-ontology#CreativeAction> .

<http://example.org/music-ontology#hasEndTime> a rdf:Property ;
    rdfs:label "music-ontology has End Time" ;
    kgqa:count 1 ;
    rdfs:comment "The end time of a creative process." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range xsd:dateTime .

<http://example.org/music-ontology#hasLocation> a rdf:Property ;
    rdfs:label "music-ontology has Location" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a process to its location." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range <http://example.org/music-ontology#Location> .

<http://example.org/music-ontology#hasStartTime> a rdf:Property ;
    rdfs:label "music-ontology has Start Time" ;
    kgqa:count 1 ;
    rdfs:comment "The start time of a creative process." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range xsd:dateTime .

<http://example.org/music-ontology#involvesAgent> a rdf:Property ;
    rdfs:label "music-ontology involves Agent" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a creative process to an agent involved in it." ;
    rdfs:domain <http://example.org/music-ontology#CreativeProcess> ;
    rdfs:range <http://example.org/music-ontology#Agent> .

<http://example.org/music-ontology#involvesEquipment> a rdf:Property ;
    rdfs:label "music-ontology involves Equipment" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a recording process to the equipment used in it." ;
    rdfs:domain <http://example.org/music-ontology#RecordingProcess> ;
    rdfs:range <http://example.org/music-ontology#Equipment> .

<http://example.org/music-ontology#produces> a rdf:Property ;
    rdfs:label "music-ontology produces" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a recording process to the recording it produces." ;
    rdfs:domain <http://example.org/music-ontology#RecordingProcess> ;
    rdfs:range <http://example.org/music-ontology#Recording> .

<http://example.org/music-ontology#receivedAward> a rdf:Property ;
    rdfs:label "music-ontology received Award" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a music artist to an award they have received." ;
    rdfs:domain <http://example.org/music-ontology#MusicArtist> ;
    rdfs:range <http://example.org/music-ontology#Award> ;
    rdfs:subClassOf <http://example.org/music-ontology#receivedNominationFor> .

<http://example.org/music-ontology#recorded> a rdf:Property ;
    rdfs:label "music-ontology recorded" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a recording process to the musical performance it recorded." ;
    rdfs:domain <http://example.org/music-ontology#RecordingProcess> ;
    rdfs:range <http://example.org/music-ontology#MusicalPerformance> .

owl:Ontology a owl:Class ;
    rdfs:label "owl Ontology" ;
    kgqa:count 1 .

owl:disjointWith a rdf:Property ;
    rdfs:label "owl disjoint With" ;
    kgqa:count 5 ;
    rdfs:domain owl:Class ;
    rdfs:range owl:Class .

owl:minCardinality a rdf:Property ;
    rdfs:label "owl min Cardinality" ;
    kgqa:count 9 ;
    rdfs:domain owl:Restriction .

owl:onProperty a rdf:Property ;
    rdfs:label "owl on Property" ;
    kgqa:count 10 ;
    rdfs:domain owl:Restriction ;
    rdfs:range owl:DatatypeProperty,
        owl:ObjectProperty .

owl:someValuesFrom a rdf:Property ;
    rdfs:label "owl some Values From" ;
    kgqa:count 1 ;
    rdfs:domain owl:Restriction ;
    rdfs:range owl:Class .

<http://example.org/music-ontology#Artist> a owl:Class ;
    rdfs:label "music-ontology Artist" ;
    kgqa:count 1 ;
    rdfs:comment "A person who creates works of art." ;
    rdfs:subClassOf <http://example.org/music-ontology#Agent> .

<http://example.org/music-ontology#CreativeAction> a owl:Class ;
    rdfs:label "music-ontology Creative Action" ;
    kgqa:count 1 ;
    rdfs:comment "A specific action or step within a creative process." .

<http://example.org/music-ontology#Equipment> a owl:Class ;
    rdfs:label "music-ontology Equipment" ;
    kgqa:count 1 ;
    rdfs:comment "Tools or machinery used in a process." .

<http://example.org/music-ontology#Location> a owl:Class ;
    rdfs:label "music-ontology Location" ;
    kgqa:count 1 ;
    rdfs:comment "A geographical or abstract place." .

<http://example.org/music-ontology#MusicalPerformance> a owl:Class ;
    rdfs:label "music-ontology Musical Performance" ;
    kgqa:count 1 ;
    rdfs:comment "The act of performing music." .

<http://example.org/music-ontology#MusicalPiece> a owl:Class ;
    rdfs:label "music-ontology Musical Piece" ;
    kgqa:count 1 ;
    rdfs:comment "An abstract musical composition or work." .

<http://example.org/music-ontology#Recording> a owl:Class ;
    rdfs:label "music-ontology Recording" ;
    kgqa:count 1 ;
    rdfs:comment "A captured audio or visual artifact, typically produced by a recording process." .

<http://example.org/music-ontology#receivedNominationFor> a rdf:Property ;
    rdfs:label "music-ontology received Nomination For" ;
    kgqa:count 1 ;
    rdfs:comment "Relates a music artist to an award they were nominated for." ;
    rdfs:domain <http://example.org/music-ontology#MusicArtist> ;
    rdfs:range <http://example.org/music-ontology#Award> .

owl:DatatypeProperty a owl:Class ;
    rdfs:label "owl Datatype Property" ;
    kgqa:count 1 .

owl:ObjectProperty a owl:Class ;
    rdfs:label "owl Object Property" ;
    kgqa:count 1 .

<http://example.org/music-ontology#Agent> a owl:Class ;
    rdfs:label "music-ontology Agent" ;
    kgqa:count 1 ;
    rdfs:comment "An entity capable of action, such as a person or organization." .

<http://example.org/music-ontology#Award> a owl:Class ;
    rdfs:label "music-ontology Award" ;
    kgqa:count 1 ;
    rdfs:comment "A recognition or prize given for achievement." .

<http://example.org/music-ontology#MusicArtist> a owl:Class ;
    rdfs:label "music-ontology Music Artist" ;
    kgqa:count 1 ;
    rdfs:comment "An artist specializing in music." ;
    rdfs:subClassOf <http://example.org/music-ontology#Artist> .

<http://example.org/music-ontology#Process> a owl:Class ;
    rdfs:label "music-ontology Process" ;
    kgqa:count 1 ;
    rdfs:comment "A series of actions or steps taken in order to achieve a particular end." .

owl:Restriction a owl:Class ;
    rdfs:label "owl Restriction" ;
    kgqa:count 1 .

<http://example.org/music-ontology#RecordingProcess> a owl:Class ;
    rdfs:label "music-ontology Recording Process" ;
    kgqa:count 1 ;
    rdfs:comment "A process involving the capture of sound or visual data." ;
    rdfs:subClassOf <file:///C:/Users/sgwmorli/internship_stage_2/animl_ontology_2/superclass>,
        <http://example.org/music-ontology#Process> .

<http://example.org/music-ontology#CreativeProcess> a owl:Class ;
    rdfs:label "music-ontology Creative Process" ;
    kgqa:count 1 ;
    rdfs:comment "A process that leads to the creation of a new work, such as a musical piece." ;
    rdfs:subClassOf <file:///C:/Users/sgwmorli/internship_stage_2/animl_ontology_2/superclass>,
        <http://example.org/music-ontology#Process> .

owl:Class a owl:Class ;
    rdfs:label "owl Class" ;
    kgqa:count 1 .

