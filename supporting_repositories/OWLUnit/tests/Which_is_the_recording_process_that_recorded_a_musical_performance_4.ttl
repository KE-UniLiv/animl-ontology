
            @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
            @prefix foaf: <http://xmlns.com/foaf/0.1/> .
            @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

            owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	            owlunit:hasCompetencyQuestion "Which is the recording process that recorded a musical performance?" ;
 	            owlunit:hasSPARQLUnitTest """
	            
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX music-ontology: <http://example.org/music-ontology#>

SELECT ?recordingProcess ?label WHERE {
    ?recordingProcess a music-ontology:RecordingProcessThatRecordedMusicalPerformance .
    OPTIONAL { 
        ?recordingProcess rdfs:label ?label.
    }
}
""" ;
	            owlunit:testsOntology <../../../data/ontologies/synthetic_small_test.ttl> .