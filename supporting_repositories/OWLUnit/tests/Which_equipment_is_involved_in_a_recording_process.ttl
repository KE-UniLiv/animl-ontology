
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which equipment is involved in a recording process?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?equipment
WHERE {
  ?recordingProcess rdf:type <https://w3id.org/polifonia/ontology/music-meta/RecordingProcess> ;
                    <https://w3id.org/polifonia/ontology/music-meta/hasEquipment> ?equipment .
  ?equipment rdf:type <https://w3id.org/polifonia/ontology/music-meta/RecordingEquipment> .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .