
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Where did a publication situation happen?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX ns1: <http://www.kgqa.com/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?locationLabel
WHERE {
    ?publicationSituation rdf:type <https://w3id.org/polifonia/ontology/music-meta/PublicationSituation> .
    ?publicationSituation <https://w3id.org/polifonia/ontology/music-meta/happenedAt> ?location .
    ?location rdfs:label ?locationLabel
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .