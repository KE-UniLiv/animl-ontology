
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which award has a music artist received a nomination for?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX ns1: <http://www.kgqa.com/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?awardLabel
WHERE {
  ?artist rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicArtist> ;
          <https://w3id.org/polifonia/ontology/music-meta/nominatedForAward> ?award .
  ?award rdf:type <https://w3id.org/polifonia/ontology/music-meta/Award> ;
         rdfs:label ?awardLabel .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .