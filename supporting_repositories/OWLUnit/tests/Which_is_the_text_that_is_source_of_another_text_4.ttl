
            @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
            @prefix foaf: <http://xmlns.com/foaf/0.1/> .
            @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

            owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	            owlunit:hasCompetencyQuestion "Which is the text that is source of another text?" ;
 	            owlunit:hasSPARQLUnitTest """
	            
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ns1: <http://www.kgqa.com/>
PREFIX music-ontology: <http://example.org/music-ontology#>

SELECT ?sourceText ?derivedText WHERE {
    ?derivedText a music-ontology:Recording .  # Assuming derivedText refers to a recorded piece
    ?derivedText music-ontology:creates ?sourceText .  # Assuming this represents the relationship you're looking for
    FILTER(REGEX(?sourceText, "text", "i"))  # Filter to ensure that the source text is related to "text"
}
""" ;
	            owlunit:testsOntology <../../../data/ontologies/synthetic_small_test.ttl> .