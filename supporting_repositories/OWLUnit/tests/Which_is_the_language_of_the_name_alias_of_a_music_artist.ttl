
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which is the language of the name/alias of a music artist?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?language
WHERE {
  ?artist rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicArtist> .
  ?artist <https://w3id.org/polifonia/ontology/core/name> ?name .
  ?artist <https://w3id.org/polifonia/ontology/core/Alias> ?alias .
  BIND(lang(?name) AS ?language)
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .