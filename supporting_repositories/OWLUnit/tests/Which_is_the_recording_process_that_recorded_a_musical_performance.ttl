
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which is the recording process that recorded a musical performance?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?recordingProcess
WHERE {
  ?musicalPerformance rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicalPerformance> ;
                    <https://w3id.org/polifonia/ontology/music-meta/isRecordedBy> ?recordingProcess .
  ?recordingProcess rdf:type <https://w3id.org/polifonia/ontology/music-meta/RecordingProcess> .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .