            owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	            owlunit:hasCompetencyQuestion "Which is the start date of the activity of a music artist?" ;
 	            owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ns1: <http://www.kgqa.com/>
PREFIX music-ontology: <http://example.org/music-ontology#>

SELECT ?artist ?startTime WHERE {
    ?artist a music-ontology:MusicArtist .  # Assuming MusicArtist is a relevant class
    ?artist music-ontology:hasStartTime ?startTime .  # Property to get the start time of the artist's activity
}
""" ;
	            owlunit:testsOntology <../../../data/ontologies/synthetic_small_test.ttl> .