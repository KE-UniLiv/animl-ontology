
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which is the form type of an abstract score?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?formType
WHERE {
  <https://w3id.org/polifonia/ontology/music-meta/FormType> rdf:type ?formType ;
                                                           <https://w3id.org/polifonia/ontology/music-meta/isFormTypeOf> <https://w3id.org/polifonia/ontology/music-meta/AbstractScore> .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .