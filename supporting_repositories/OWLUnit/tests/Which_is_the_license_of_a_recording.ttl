
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which is the license of a recording?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
                                                    
PREFIX ns1: <http://www.kgqa.com/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?licenseLabel
WHERE {
    ?recording rdf:type <https://w3id.org/polifonia/ontology/music-meta/Recording> .
    ?recording <https://w3id.org/polifonia/ontology/music-meta/hasLicense> ?license .
    ?license rdfs:label ?licenseLabel
}

                                                """ ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .