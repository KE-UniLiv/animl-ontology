
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which music artist was a music artist influenced by?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?artist1 ?artist2
WHERE {
  ?artist1 rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicArtist> ;
           <https://w3id.org/polifonia/ontology/music-meta/isInfluencedBy> ?artist2 .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .