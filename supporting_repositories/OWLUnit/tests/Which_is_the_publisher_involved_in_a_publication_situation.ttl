
        @prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
        @prefix foaf: <http://xmlns.com/foaf/0.1/> .
        @prefix owlunittest: <https://w3id.org/OWLunit/test/> .

        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which is the publisher involved in a publication situation?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?publisher
WHERE {
  ?publicationSituation rdf:type <https://w3id.org/polifonia/ontology/music-meta/PublicationSituation> ;
                        <https://w3id.org/polifonia/ontology/music-meta/hasPublisher> ?publisher .
  ?publisher rdf:type <https://w3id.org/polifonia/ontology/music-meta/Publisher> .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .