        owlunittest:primary  a owlunit:CompetencyQuestionVerification ;
 	        owlunit:hasCompetencyQuestion "Which are the parts of a music entity?" ;
 	        owlunit:hasSPARQLUnitTest """
	            
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?part
WHERE {
  ?part rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicEntity> .
  ?part <https://w3id.org/polifonia/ontology/music-meta/isPartOfMusicEntity> ?entity .
}
""" ;
	        owlunit:testsOntology <../../../data/ontologies/ontology.ttl> .