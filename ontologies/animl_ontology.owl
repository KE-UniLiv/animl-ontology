    <!-- a type of sample containing sample -->

    <owl:NamedIndividual rdf:about="http://www.w3id.org/animl/ontology/24_Wells">
        <rdfs:comment xml:lang="en">24 wells container type</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3id.org/animl/ontology/ContainerType"/>
    </owl:NamedIndividual>
    


