                    <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/Reference"/>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>


        <!-- this is an enumeration class, e.i a class that exists only to reify the enumeration of a property of another class,
        in this case the individuals for this class correspond to the possible data types for a datapoint -->
        <owl:Class rdf:about="http://www.w3id.org/animl/ontology/DataType">
            <rdfs:comment xml:lang="en">This is an enumeration class, e.i a class that exists only to reify the enumeration of a property of another class, in this case the individuals for this class correspond to the possible data types for a datapoint</rdfs:comment>
