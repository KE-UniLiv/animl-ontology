    <!-- this component represents the animl technique schema, or arguably meta schema, which restricts the format of technique definition documents.
    
    the purpose of a technique definition government is conceptually to express the steps involved in a type of scientific operation, and practically
    to govern certain of the properties for elements involved in an experiment step
    
    the effect of an ATDD file on an animl knowledge base can be expressed as a set of object specifications, themsleves objects with ispart relations to 
    experiment step related objects. with these specification objects being the domain for the relationships and data (regarding the object specified) that are
    pre determined by the ATDD.
    
    since the technique file is itself a specification for ATDD files, it can be expressed as an ontology for the specification objects-->

        <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

         <!-- denotes the relationship contained within the context of the specification for a referenced item,
    a relationship which indicates the requirement for the referenced item to have been consumed in a particular technique
    here technique may be viewed in its form as a specification for an experiment step -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/consumedBy">
        <rdfs:comment xml:lang="en">Denotes the relationship contained within the context of the specification for a referenced item, a relationship which indicates the requirement for the referenced item to have been consumed in a particular technique here technique may be viewed in its form as a specification for an experiment step</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSpecification"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
    </owl:ObjectProperty>
    


    <!-- this is the inverse of the above relation, and denotes the requirement for a technique to consume the referenced item of a referenec
    in order for that item to be referenced
    this relationship is not entirely intuitive, but it is necessary to preserve expressivity found in the original xml -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/consumes">
        <rdfs:comment xml:lang="en">Denotes the requirement for a technique to consume the referenced item of a referenec in order for that item to be referenced this relationship is not entirely intuitive, but it is necessary to preserve expressivity found in the original xml</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- denotes the relationship contained within the context of the specification for a referenced item,
    a relationship which indicates the requirement for the referenced item to have been derived from a particular technique
    here technique may be viewed in its form as a specification for an experiment step  -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/derivedFrom">
        <rdfs:comment xml:lang="en">Denotes the relationship contained within the context of the specification for a referenced item, a relationship which indicates the requirement for the referenced item to have been derived from a particular technique here technique may be viewed in its form as a specification for an experiment step</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSpecification"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
    </owl:ObjectProperty>
    


    <!-- this is the inverse of the above relation, and denotes the requirement for a technique to derive the referenced item of a referenec
    in order for that item to be referenced
    this relationship is not entirely intuitive, but it is necessary to preserve expressivity found in the original xml -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/derives">
        <rdfs:comment xml:lang="en">Denotes the requirement for a technique to derive the referenced item of a referenec in order for that item to be referenced this relationship is not entirely intuitive, but it is necessary to preserve expressivity found in the original xml</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSpecification"/>
    </owl:ObjectProperty>


        <!-- this relationship denotes that the 'extension' (itself a subclass of technique) exists to:
    extend the technqiue if you consider techniques as independent
    complete the specification for an experiment step, if you consider technqieus as specifications for experiment steps -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/extends">
        <rdfs:comment xml:lang="en">Denotes that the 'extension' (itself a subclass of technique) exists to: extend the technqiue if you consider techniques as independent complete the specification for an experiment step, if you consider technqieus as specifications for experiment steps</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
    </owl:ObjectProperty>
    

    <!-- this is the inverse of the above relation, denoting that a technique is extended or completed by a particular extension -->



    <!-- denotes that a class is a container for data with corresponding units of measurement, a complex quantity may have multiple units
    e.i mph, or rpm -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/hasUnits">
        <rdfs:comment xml:lang="en">Denotes that a class is a container for data with corresponding units of measurement, a complex quantity may have multiple units e.i mph, or rpm</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ParameterSpecification"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/Units"/>
    </owl:ObjectProperty>

        <!-- conceptually this relationship is the property of being bound to a specific data type, which forces classes representing data containers
    to have individuals that only contain data of specific types
    
    however it is attached to the specification for such containers, since the type of data a container may hold is governed by its specification -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/hasDataType">
        <rdfs:comment xml:lang="en">Conceptually this relationship is the property of being bound to a specific data type, which forces classes representing data containers to have individuals that only contain data of specific types however it is attached to the specification for such containers, since the type of data a container may hold is governed by its specification</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ParameterSpecification"/>
        <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/DataType"/>
    </owl:ObjectProperty>
    

            <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

         <!-- a constant value added to the equation resulting from the combination of data with units -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/offset">
        <rdfs:comment xml:lang="en">A constant value added to the equation resulting from the combination of data with units</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/SIUnit"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        </owl:DatatypeProperty>
    


        <!-- an english language description of type of objects or measurements to which units may be assigned -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/quantity">
        <rdfs:comment xml:lang="en">A description of type of objects or measurements to which units may be assigned</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Units"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        </owl:DatatypeProperty>

        <!-- a raised power, typically used to raise the dimensionality of spacial data -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/exponent">
            <rdfs:comment xml:lang="en">A raised power, typically used to raise the dimensionality of spacial data</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/SIUnit"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        </owl:DatatypeProperty>
    
        <!-- a coefficient upon a set of units, typically an order of magnitude -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/factor">
            <rdfs:comment xml:lang="en">A coefficient upon a set of units, typically an order of magnitude</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/SIUnit"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        </owl:DatatypeProperty>

        <!-- a string space for the bibliography related to an object -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/citations">
            <rdfs:comment xml:lang="en">A string space for the bibliography related to an object</rdfs:comment>
            <rdfs:domain>
                <owl:Class>
                    <owl:unionOf rdf:parseType="Collection">
                        <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Technique"/>
                        <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/ItemSpecification"/>
                    </owl:unionOf>
                </owl:Class>
            </rdfs:domain>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>

        <!-- a string space for a software requirement enumeration for an object -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/dependency">
            <rdfs:comment xml:lang="en">A string space for a software requirement enumeration for an object</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/SeriesSpecification"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>

        <!-- the semantic web identifier for a the superclass of technique and extension, used as a path to import an ATDD to an animl file -->


        <!-- a boolean denoting whether or not a technique or extension will work without being extended -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/abstract">
            <rdfs:comment xml:lang="en">A boolean denoting whether or not a technique or extension will work without being extended</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        </owl:DatatypeProperty>

        <!-- a unique token used to authenticate identity of an ATDD for security purposes -->

        <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/token">
            <rdfs:comment xml:lang="en">A unique token used to authenticate identity of an ATDD for security purposes</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Technique"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#token"/>
        </owl:DatatypeProperty>


