     -->


     <!-- this pattern is a generalisation of the behaviour (found at multiple points in the animl xml) of classes, typically
     classes representing activities or information supporting activities, to be related by a number of methods to other classes,
     typically representing persistent data within containers, which cannot be directly related to them since they are members of indexed
     data structures or containers
     
     the core of this pattern is the reference set, and the references it contains. the reference set corresponds to a data structure in which
     all the referenced data points reside, and each reference corresponds to each data point.
     
     certain information regarding the relationship of the referenced information to the object referencing it is also reified as object properties
     with domain in the reference class.
     
     a class which references other classes includes a reference set-->

        <!--object properties-->
            <!-- this relation denotes that the referenceset in the domain contains references to data within a data structure
            of the range class -->

            <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/subject">
                <rdfs:comment xml:lang="en">This relation denotes that the referenceset in the domain contains references to data within a data structure of the range class</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSet"/>
                <rdfs:range>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Experiment"/>
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/SampleSet"/>
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Series"/>
                        </owl:unionOf>
                    </owl:Class>
                </rdfs:range>
            </owl:ObjectProperty>


            <!-- this indicates that the domain reference points to something of the range class -->

            <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/pointsTo">
                <rdfs:comment xml:lang="en">This indicates that the domain reference points to something of the range class</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Reference"/>
                <rdfs:range>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/DataPoint"/>
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/ExperimentStep"/>
                            <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Sample"/>
                        </owl:unionOf>
                    </owl:Class>
                </rdfs:range>
            </owl:ObjectProperty>


            <!-- this relation represents a reified data property, where a reference is related to the textual description of
            the role of the data it points to, this is an optional property distinct from function and the consumes/derives relationship,
            it is used when further infromation about the purpose of the data is necessary to store  -->

            <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/hasUseCase">
                <rdfs:comment xml:lang="en">This relation represents a reified data property, where a reference is related to the textual description of the role of the data it points to, this is an optional property distinct from function and the consumes/derives relationship, it is used when further infromation about the purpose of the data is necessary to store</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Reference"/>
                <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/UseCase"/>
            </owl:ObjectProperty>

            <!-- this relation represents a reified data property, where a reference is related to the textual description of
            the function of the data it points to -->

            <owl:ObjectProperty rdf:about="http://www.w3id.org/animl/ontology/hasFunction">
                <rdfs:comment xml:lang="en">This relation represents a reified data property, where a reference is related to the textual description of the function of the data it points to</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/Reference"/>
                <rdfs:range rdf:resource="http://www.w3id.org/animl/ontology/Function"/>
            </owl:ObjectProperty>

        <!--data properties-->

            <!-- this indicates the first position in an ordered set that a contained item
            will be pointed to by a reference in the domain reference set  -->

            <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/startValue">
                <rdfs:comment xml:lang="en">This indicates the first position in an ordered set that a contained item will be pointed to by a reference in the domain reference set</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSet"/>
                <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:DatatypeProperty>

            <!-- this indicates the last position in an ordered set that a contained item
            will be pointed to by a reference in the domain reference set -->

            <owl:DatatypeProperty rdf:about="http://www.w3id.org/animl/ontology/endValue">
                <rdfs:comment xml:lang="en">This indicates the last position in an ordered set that a contained item will be pointed to by a reference in the domain reference set</rdfs:comment>
                <rdfs:domain rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSet"/>
                <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:DatatypeProperty>

        <!--objects-->

            <!-- this is a reference set as discussed in the header for this odp, note that it can only look
            within one data structure -->
            <owl:Class rdf:about="http://www.w3id.org/animl/ontology/ReferenceSet">
                <rdfs:comment xml:lang="en">This is a reference set as discussed in the header for this odp, note that it can only look within one data structure</rdfs:comment>
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/subject"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Series"/>
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Experiment"/>
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/SampleSet"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </rdfs:subClassOf>

            <!-- Max cardinality restriction -->
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/pointsTo"/>
                        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </rdfs:subClassOf>

                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/Reference"/>
                    </owl:Restriction>
                </rdfs:subClassOf>
            </owl:Class>

            <!-- this is a referenec as discussed in the header for this odp, note that it can
            only point to one piece of data -->
            <owl:Class rdf:about="http://www.w3id.org/animl/ontology/Reference">
                <rdfs:comment xml:lang="en">This is a reference for the odp, note that it can only point to one piece of data</rdfs:comment>
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/hasFunction"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/Function"/>
                    </owl:Restriction>
                </rdfs:subClassOf>
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/hasSpecification"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSpecification"/>
                    </owl:Restriction>
                </rdfs:subClassOf>
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/hasUseCase"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/UseCase"/>
                    </owl:Restriction>
                </rdfs:subClassOf>

                <!-- Combined allValuesFrom restriction using owl:unionOf -->
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/pointsTo"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/DataPoint"/>
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/ExperimentStep"/>
                                    <rdf:Description rdf:about="http://www.w3id.org/animl/ontology/Sample"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </rdfs:subClassOf>


                <!-- Max cardinality restriction -->
                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/animl/ontology/pointsTo"/>
                        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </rdfs:subClassOf>

                <rdfs:subClassOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3id.org/animl/ontology/ReferenceSet"/>
                    </owl:Restriction>
                </rdfs:subClassOf>
            </owl:Class>
            
            <!-- this is a reified natural language field for the use case of a set of referenced datapoints  -->
            <owl:Class rdf:about="http://www.w3id.org/animl/ontology/UseCase">
                <rdfs:comment xml:lang="en">This is a reified natural language field for the use case of a set of referenced datapoints</rdfs:comment>
            </owl:Class>


            <!-- this is a reified natural language field for the 'function' of data when being referenced
            this is optional and works to distinguish the general purpose of data from its immediate use
            
            this is an artefact from the construction of owl that has been left in to avoid damaging expressivity -->
            <owl:Class rdf:about="http://www.w3id.org/animl/ontology/Function">
                <rdfs:comment xml:lang="en">This is a reified natural language field for the 'function' of data when being referenced this is optional and works to distinguish the general purpose of data from its immediate use this is an artefact from the construction of owl that has been left in to avoid damaging expressivity</rdfs:comment>
            </owl:Class>
</rdf:RDF>
