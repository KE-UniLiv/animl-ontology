     <!-- animl reference pattern
     this pattern is a generalisation of the behaviour (found at multiple points in the animl xml) of classes, typically
     classes representing activities or information supporting activities, to be related by a number of methods to other classes,
     typically representing persistent data within containers, which cannot be directly related to them since they are members of indexed
     data structures or containers
     
     the core of this pattern is the reference set, and the references it contains. the reference set corresponds to a data structure in which
     all the referenced data points reside, and each reference corresponds to each data point.
     
     certain information regarding the relationship of the referenced information to the object referencing it is also reified as object properties
     with domain in the reference class.
     
     a class which references other classes includes a reference set-->

        <!--object properties-->
            <!-- this relation denotes that the referenceset in the domain contains references to data within a data structure
            of the range class -->

            <owl:ObjectProperty rdf:about="http://w3id.org/animl/lookingWithin">
                <rdfs:domain rdf:resource="https://w3id.org/animl/referenceSet"/>
                <rdfs:range rdf:resource="https://w3id.org/animl/experiment"/>
                <rdfs:range rdf:resource="https://w3id.org/animl/sampleSet"/>
                <rdfs:range rdf:resource="https://w3id.org/animl/series"/>
            </owl:ObjectProperty>


            <!-- this indicates that the domain reference points to something of the range class -->

            <owl:ObjectProperty rdf:about="http://w3id.org/animl/pointsTo">
                <rdfs:domain rdf:resource="http://w3id.org/animl/reference"/>
                <rdfs:range rdf:resource="http://w3id.org/animl/datapoint"/>
                <rdfs:range rdf:resource="https://w3id.org/animl/experimentStep"/>
                <rdfs:range rdf:resource="https://w3id.org/animl/datapoint"/>
            </owl:ObjectProperty>


            <!-- this relation represents a reified data property, where a reference is related to the textual description of
            the role of the data it points to, this is an optional property distinct from function and the consumes/derives relationship,
            it is used when further infromation about the purpose of the data is necessary to store  -->

            <owl:ObjectProperty rdf:about="http://w3id.org/animl/hasRole">
                <rdfs:domain rdf:resource="http://w3id.org/animl/reference"/>
                <rdfs:range rdf:resource="http://w3id.org/animl/role"/>
            </owl:ObjectProperty>

            <!-- this relation represents a reified data property, where a reference is related to the textual description of
            the function of the data it points to -->

            <owl:ObjectProperty rdf:about="http://w3id.org/animl/hasFunction">
                <rdfs:domain rdf:resource="http://w3id.org/animl/reference"/>
                <rdfs:range rdf:resource="http://w3id.org/animl/function"/>
