@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owlunittest: <https://w3id.org/OWLunit/test/> .

owlunittest:death  a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which award has a music artist received?" ;
 	owlunit:hasSPARQLUnitTest """	PREFIX mm: <https://w3id.org/polifonia/ontology/music-meta/>
	 SELECT DISTINCT ?award ?musicArtist WHERE {?musicArtist a mm:MusicArtist ; mm:receivedAward ?award}""" ;
	owlunit:testsOntology <data/ontologies/test_ontology.ttl> . 