@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owlunittest: <https://w3id.org/OWLunit/test/> .

owlunittest:death  a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which award has a music artist received a nomination for?" ;
 	owlunit:hasSPARQLUnitTest """
	PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
	PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
	PREFIX ns1: <http://www.kgqa.com/>
	
	SELECT ?award WHERE {
    ?musicArtist rdf:type <https://w3id.org/polifonia/ontology/music-meta/MusicArtist> .
    ?musicArtist <https://w3id.org/polifonia/ontology/music-meta/nominatedForAward> ?award .
	}""" ;
	owlunit:testsOntology <data/ontologies/ontology.ttl> . 