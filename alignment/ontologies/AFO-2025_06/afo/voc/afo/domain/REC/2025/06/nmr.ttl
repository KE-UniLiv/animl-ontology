###  http://purl.allotrope.org/ontologies/equipment#AFE_0002280
af-e:AFE_0002280 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000857 ;
                 skos:altLabel "NMR spectrometer" ;
                 skos:definition "A nuclear magnetic resonance spectrometer is a measurement device used to conduct NMR experiments consisting of a magnet housing a sample space surrounded by two coils which act to transmit radio frequency radiation to the sample and to detect the response of the sample. [CHMO]" ;
                 skos:prefLabel "nuclear magnetic resonance spectrometer" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0002281
af-e:AFE_0002281 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000718 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0000056 ;
                                   owl:someValuesFrom af-p:AFP_0000326
                                 ] ;
                 skos:altLabel "NMR tube" ;
                 skos:definition "A nuclear magnetic resonance tube is a tube that is used in an NMR experiment. [Allotrope]" ;
                 skos:prefLabel "nuclear magnetic resonance tube" .


###  http://purl.allotrope.org/ontologies/material#AFM_0001135
af-m:AFM_0001135 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000012 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0000087 ;
                                   owl:someValuesFrom [ owl:intersectionOf ( af-rl:AFRL_0000269
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty bfo:_0000054 ;
                                                                               owl:someValuesFrom af-p:AFP_0000326
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
                 skos:altLabel "NMR solvent" ;
                 skos:definition "A nuclear magnetic resonance solvent is a solution used to dissolve a sample in an NMR assay. It typically has all / most hydrogen atoms replaced with deuterium atoms. [Allotrope]" ;
                 skos:prefLabel "nuclear magnetic resonance solvent" .


