###  http://purl.allotrope.org/ontologies/process#AFP_0000843
af-p:AFP_0000843 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000588 ;
                 af-x:AFX_0002809 <http://purl.obolibrary.org/obo/CHMO_0001013> ;
                 skos:altLabel "SEC" ,
                               "size exclusion chromatography" ;
                 skos:definition "Column chromatography where the separation is achieved based on differences in molecular size. [CHMO]" ;
                 skos:prefLabel "size-exclusion chromatography" .


###  http://purl.allotrope.org/ontologies/role#AFRL_0000571
af-rl:AFRL_0000571 rdf:type owl:Class ;
                   rdfs:subClassOf af-rl:AFRL_0000255 ;
                   af-x:AFX_0002798 "true"^^xsd:boolean ;
                   skos:definition "A narrow standard role is a standard sample role used in size-exclusion chromatography that has a narrow molecular-weight distribution and are typically chemically different than the sample. [Allotrope]" ;
                   skos:prefLabel "narrow standard role (size-exclusion chromatography)" .


###  http://purl.allotrope.org/ontologies/role#AFRL_0000572
af-rl:AFRL_0000572 rdf:type owl:Class ;
                   rdfs:subClassOf af-rl:AFRL_0000255 ;
                   af-x:AFX_0002798 "true"^^xsd:boolean ;
                   skos:definition "A broad standard role is a standard sample role used in size-exclusion chromatography that has a broad molecular-weight distribution and are usually the same polymer as the sample being analyzed. [Allotrope]" ;
                   skos:prefLabel "broad standard role (size-exclusion chromatography)" .


###  http://purl.obolibrary.org/obo/PATO_0000918
<http://purl.obolibrary.org/obo/PATO_0000918> rdf:type owl:Class .


