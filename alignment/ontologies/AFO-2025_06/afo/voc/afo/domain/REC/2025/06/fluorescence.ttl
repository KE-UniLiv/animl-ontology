                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A fluorescent tag identifier is an identifier for the type of molecules having the role of fluorescent tag. [Allotrope]" ;
                 skos:prefLabel "fluorescent tag identifier" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002964
af-r:AFR_0002964 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0000576 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty af-x:AFX_0002816 ;
                                   owl:someValuesFrom af-p:AFP_0003846
                                 ] ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A fluorescence emission profile is a profile that quantifies the fluorescence emission of a material entity over time. [Allotrope]" ;
                 skos:prefLabel "fluorescence emission profile" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002965
