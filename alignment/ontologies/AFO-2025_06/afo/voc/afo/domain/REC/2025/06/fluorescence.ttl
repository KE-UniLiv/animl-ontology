                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf af-x:AFX_0002737
                                                  ] ;
                                   owl:someValuesFrom af-r:AFR_0003211
                                 ] ;
                 skos:definition "A fluorescence area scan data cube is a data cube that represents a fluorescence area scan. [Allotrope]" ;
                 skos:prefLabel "fluorescence area scan data cube" .


###  http://purl.allotrope.org/ontologies/result#AFR_0003211
af-r:AFR_0003211 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0002499 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
