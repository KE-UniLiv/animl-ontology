                 skos:altLabel "TXRF" ,
                               "total reflection X-ray fluorescence analysis" ,
                               "total reflection X-ray fluorescence spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000577
af-p:AFP_0000577 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001749 ;
                 skos:altLabel "fluorescence polarisation spectroscopy"@en-gb ,
                               "FA" ,
                               "fluorescence anisotropy measurement " ,
                               "fluorescence polarization spectroscopy" ;
                 skos:definition "A method for measuring the binding interaction between two labelled molecules. The sample is excited with vertically polarized light and the polarization of the fluorescence is measured. [CHMO]" ;
                 skos:prefLabel "fluorescence anisotropy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000738
af-p:AFP_0000738 rdf:type owl:Class ;
