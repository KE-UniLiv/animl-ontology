                 rdfs:subClassOf af-p:AFP_0002099 ;
                 skos:altLabel "GP spectroscopy" ;
                 skos:definition "A type of spectroscopy where a spectral shift, in fluorescence spectrum, which depends on the phase of lipid membranes is measured. [CHMO]" ;
                 skos:prefLabel "generalised polarisation spectroscopy"@en-gb ,
                                "generalized polarization spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001217
af-p:AFP_0001217 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002099 ;
                 skos:altLabel "time resolved fluorescence spectroscopy" ,
                               "time resolved fluorimetry" ,
                               "time resolved fluorometry" ,
                               "time resolved spectrofluorometry" ,
                               "time-resolved fluorimetry" ,
                               "time-resolved fluorometry" ,
                               "time-resolved spectrofluorometry" ;
                 skos:definition "A type of spectroscopy where the time decay of the fluorescence of molecules excited using ultraviolet light is detected. [CHMO]" ;
                 skos:prefLabel "time-resolved fluorescence spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001322
af-p:AFP_0001322 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002432 ;
                 skos:altLabel "fluorescence imaging" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001369
af-p:AFP_0001369 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "FCCS" ,
                               "fluorescence cross-correlation spectrometry" ;
                 skos:definition "A type of spectroscopy where spontaneous fluorescence intensity fluctuations of a sample are measured at two distinct wavelengths and correlated within a very small detection volume. Ultra-violet light is used to stimulate fluorescence. [CHMO]" ;
                 skos:prefLabel "fluorescence cross-correlation spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001388
af-p:AFP_0001388 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001782 ;
                 skos:altLabel "BRET" ;
                 skos:definition "A method for determining the distance between molecules using an energy transfer between two chromophores (a donor and an acceptor) in close proximity (typically <10 nm) via a nonradiative, long-range dipole-dipole coupling mechanism. Energy transfer usually occurs between a luciferase (donor) and a green fluorescent protein (acceptor). The luciferase is bioluminescent therefore external illumination is not required. [CHMO]" ;
                 skos:prefLabel "bioluminescence resonance energy transfer" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001411
af-p:AFP_0001411 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002099 ;
                 skos:altLabel "EEM" ,
                               "excitation emission matrix (EEM) fluorescence" ,
                               "excitation-emission matrix fluorescence" ,
                               "excitation-emission matrix fluorescence spectroscopy" ,
                               "fluorescence EEM" ;
                 skos:definition "A type of fluorescence spectroscopy where a two-dimensional total fluorescence spectrum is recorded at various excitationnwavelengths. [CHMO]" ;
                 skos:prefLabel "excitation-emission matrix fluorescence spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001448
af-p:AFP_0001448 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002099 ;
                 skos:altLabel "DSF" ,
                               "differential scanning fluorimetry" ;
                 skos:definition "A type of spectroscopy where the intensity of the  fluorescence emitted by molecules excited using ultraviolet light is detected as it changes upon interaction of the molecule with its environment. [CHMO]" ;
                 skos:prefLabel "differential scanning fluorescence spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001467
af-p:AFP_0001467 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000577 ;
                 skos:altLabel "time-resolved fluorescence polarisation spectroscopy"@en-gb ,
                               "TRFA" ,
                               "time resolved FA" ,
                               "time resolved fluorescence anisotropy" ,
                               "time-resolved FA" ,
                               "time-resolved fluorescence polarization spectroscopy" ;
                 skos:definition "A method for measuring the binding interaction between two labelled molecules. The sample is excited with vertically polarized light and the decay time and polarization of the fluorescence is measured. [CHMO]" ;
                 skos:prefLabel "time-resolved fluorescence anisotropy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001556
af-p:AFP_0001556 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001749 ;
                 skos:altLabel "FRAP" ;
                 skos:definition "A method for determining the mobility of fluorescent molecules by photobleaching a sub-set of the sample during fluorescence microscopy and monitoring the movement of the remaining fluorescent molecules. [CHMO]" ;
                 skos:prefLabel "fluorescence recovery after photobleaching" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001782
af-p:AFP_0001782 rdf:type owl:Class .


###  http://purl.allotrope.org/ontologies/process#AFP_0001820
af-p:AFP_0001820 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001214 ;
                 skos:altLabel "Laurdan generalized polarisation spectroscopy"@en-gb ,
                               "generalized polarisation of Laurdan fluorescence"@en-gb ,
                               "laurdan generalized polarisation (GP) measurements"@en-gb ,
                               "Laurdan generalized polarization spectroscopy" ,
                               "generalized polarization of Laurdan fluorescence" ,
                               "laurdan fluorescence spectroscopy" ,
                               "laurdan generalized polarization (GP) measurements" ;
                 skos:definition "A type of spectroscopy where the difference between the red and blue fluorescence emission of the fluorescent probe laurdan (6-dodecanoyl-2- dimethylaminonaphthalene), which depends on the phase of lipid membranes, is measured. [CHMO]" ;
                 skos:prefLabel "laurdan generalised polarisation spectroscopy"@en-gb ,
                                "laurdan generalized polarization spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001894
af-p:AFP_0001894 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "SA-FCS" ,
                               "supercritical angle FCS" ;
                 skos:definition "A type of spectroscopy where the spontaneous fluorescence intensity fluctuations of a sample emitted at supercritical angles are measured and correlated within a very small detection volume. Ultra-violet light is used to stimulate fluorescence. [CHMO]" ;
                 skos:prefLabel "supercritical angle fluorescence correlation spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001929
af-p:AFP_0001929 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "high pressure FCS" ,
                               "high pressure fluorescence correlation spectroscopy" ,
                               "high-pressure FCS" ;
                 skos:definition "A type of spectroscopy where the spontaneous fluorescence intensity fluctuations of a sample are measured and correlated within a very small detection volume. Ultraviolet light is used to stimulate fluorescence and the sample is held inside a single fused silica microcapillary under high pressure (up to 300 MPa). [CHMO]" ;
                 skos:prefLabel "high-pressure fluorescence correlation spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001930
af-p:AFP_0001930 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001280 ;
                 skos:altLabel "USX" ,
                               "USX spectrometry" ,
                               "USX spectroscopy" ,
                               "USXES" ,
                               "USXS" ,
                               "ultra soft X-ray emission spectrometry" ,
                               "ultra soft X-ray fluorescence spectrometry" ,
                               "ultra soft X-ray fluorescence spectroscopy" ,
                               "ultra-soft X-ray emission spectrometry" ,
                               "ultra-soft X-ray fluorescence spectrometry" ,
                               "ultra-soft X-ray fluorescence spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002117
af-p:AFP_0002117 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002099 ;
                 skos:altLabel "steady state fluorescence spectroscopy" ,
                               "steady state fluorimetry" ,
                               "steady state fluorometry" ,
                               "steady state spectrofluorometry" ,
                               "steady-state fluorimetry" ,
                               "steady-state fluorometry" ,
                               "steady-state spectrofluorometry" ;
                 skos:definition "A type of spectroscopy where the intensity of the fluorescence emitted by molecules excited by constant illumination of ultraviolet light is detected as a function of wavelength. [CHMO]" ;
                 skos:prefLabel "steady-state fluorescence spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002144
af-p:AFP_0002144 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "TIR-FCS" ,
                               "total internal reflection FCS" ;
                 skos:definition "A type of spectroscopy where ultraviolet light is used to stimulate fluorescence in a sample, and the spontaneous fluorescence intensity fluctuations are measured and correlated within a very small detection volume. To generate total internal reflection the UV light is either passed through a prism to generate high enough incident angles, or the UV light is focused through the periphery of a high numerical aperture objective which is directly coupled to the sample surface. [CHMO]" ;
                 skos:prefLabel "total internal reflection fluorescence correlation spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002164
af-p:AFP_0002164 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001207 ;
                 skos:altLabel "TRFQ" ,
                               "TRFQ analysis" ,
                               "TRFQ measurements" ,
                               "TRFQ spectrometry" ,
                               "TRFQ spectroscopy" ,
                               "time resolved fluorescence quenching" ,
                               "time resolved fluorescence quenching spectrometry" ,
                               "time-resolved fluorescence quenching spectrometry" ,
                               "time-resolved fluorescence quenching spectroscopy" ;
                 skos:definition "The measurement of the fluorescence decay time of probes solubilised in micellar surfactant solutions in the presence of quenchers. [CHMO]" ;
                 skos:prefLabel "time-resolved fluorescence quenching" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002241
af-p:AFP_0002241 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "confocal FCS" ;
                 skos:definition "A type of spectroscopy where the spontaneous fluorescence intensity fluctuations of a sample are measured and correlated within a very small detection volume. The light emitted during fluorescence is collected and guided through a pinhole before it is detected, reducing background interference. [CHMO]" ;
                 skos:prefLabel "confocal fluorescence correlation spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002312
af-p:AFP_0002312 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000851 ;
                 skos:altLabel "ICS" ;
                 skos:definition "A type of spectroscopy where the spontaneous fluorescence intensity fluctuations of a sample are measured at several different points in parallel and correlated within very small detection volumes. Ultra-violet light is used to stimulate fluorescence. [CHMO]" ;
                 skos:prefLabel "image correlation spectroscopy" .


