###  http://purl.allotrope.org/ontologies/equipment#AFE_0000063
af-e:AFE_0000063 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000549 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A teflon capillary is a neutral capillary with a teflon coating on its inner surface. [Allotrope]" ;
                 skos:prefLabel "teflon capillary" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000232
af-e:AFE_0000232 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000659 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "bare silica capillary" ,
                               "uncoated silica capillary" ;
                 skos:definition "A bare fused silica capillary has no coatings on its inner or outer surface. [Allotrope]" ;
                 skos:prefLabel "bare fused silica capillary" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000549
af-e:AFE_0000549 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000340 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A neutral capillary is one that contains no anchor groups to neither anions nor cations. [Allotrope]" ;
                 skos:prefLabel "neutral capillary" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000600
af-e:AFE_0000600 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000659 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "capillary gel column" ;
                 skos:definition "A gel filled capillary is a separation capillary that contains a gel as stationary phase. [Allotrope]" ;
                 skos:prefLabel "gel filled capillary" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000659
af-e:AFE_0000659 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000340 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A fused silica capillary is a separation capillary that consists of silica in amophous form. [Allotrope]" ;
                 skos:prefLabel "fused silica capillary" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000868
af-e:AFE_0000868 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000340 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "functionalized capillary" ;
                 skos:definition "A functionalized capillary is a separation capillary that has a functionalized coating on its inner surface. [Allotrope]" ;
                 skos:prefLabel "coated capillary" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000083
af-m:AFM_0000083 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000053 ;
                 skos:altLabel "BGE" ;
                 skos:definition "The background electrolyte is the running material used in capillary electrophoresis. [Allotrope]" ;
                 skos:prefLabel "background electrolyte" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000144
af-m:AFM_0000144 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000012 ;
                 skos:altLabel "conditioner solution" ;
                 skos:definition "A capillary conditioner is a solution that is used for rinsing a capillary in order to generate a fresh state. [Allotrope]" ;
                 skos:prefLabel "capillary conditioner" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000204
af-m:AFM_0000204 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000921 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "Carrier ampholytes are zwitterionic compounds that are used for the creation of pH gradients within the capillary of capillary electrophoresis. [Allotrope]" ;
                 skos:prefLabel "carrier ampholyte" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000274
af-m:AFM_0000274 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000348 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "Chemical gel is a material which changes physical state by forming covalent attachments when mixed in an aqueous solution. [Allotrope]" ;
                 skos:prefLabel "chemical gel" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000307
af-m:AFM_0000307 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000053 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A leading electrolyte refers to an ionic analyte which is travelling at a faster rate than other analytes in a separation technique. [Allotrope]" ;
                 skos:prefLabel "leading electrolyte" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000348
af-m:AFM_0000348 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0001033 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "A capillary electrophoresis gel is a polymer network used in an electrophoresis process. [Allotrope]" ;
                 skos:example "uncrosslinked polyacrylamide, methylcellulose" ;
                 skos:prefLabel "capillary electrophoresis gel" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000349
af-m:AFM_0000349 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000348 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "Physical gels are materials which obtain their crosslinking through a form of polymer entanglement, this usually requires  heat input, and the gel settles into place as it cools. [Allotrope]" ;
                 skos:prefLabel "physical gel" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000921
af-m:AFM_0000921 rdf:type owl:Class .


###  http://purl.allotrope.org/ontologies/process#AFP_0000092
af-p:AFP_0000092 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000474 ,
                                 af-p:AFP_0002364 ;
                 skos:altLabel "ACE" ,
                               "CAE" ;
                 skos:definition "An electrophoresis method which takes place in a capillary where the separation medium has specific interactions with the analytes. [CHMO]" ;
                 skos:prefLabel "capillary affinity electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000918
af-p:AFP_0000918 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000669 ;
                 skos:definition "A capillary array electrophoresis uses many capillaries at the same time allowing many analyses at the same time. [Allotrope]" ;
                 skos:prefLabel "capillary array electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001155
af-p:AFP_0001155 rdf:type owl:Class ;
                 rdfs:subClassOf bfo:_0000015 ;
                 skos:altLabel "capillary regeneration" ;
                 skos:definition "Process by which the material of the capillary is treated to result in a uniform charge throughout the surface. [Allotrope]" ;
                 skos:example "Pretreat the capillary for 10 min with 0.1 M sodium hydroxide, 5 min with water, and 10 min with run buffer." ;
                 skos:prefLabel "capillary conditioning" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001165
af-p:AFP_0001165 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000268 ,
                                 af-p:AFP_0000474 ;
                 skos:altLabel "CSE" ;
                 skos:definition "An electrophoresis method that takes place in a capillary and where the separation medium contains a reticulated system, for example an entangled polymer network, that further separates the analytes according to their size and shape. [CHMO]" ;
                 skos:prefLabel "capillary sieving electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001342
af-p:AFP_0001342 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000365 ;
                 skos:altLabel "immobilised pH gradient-IEF"@en-gb ,
                               "IPG based-IEF" ,
                               "IPG-IEF" ,
                               "immobilized pH gradient-IEF" ;
                 skos:changeNote "2016-02-03 Switched labels [OSTHUS]" ,
                                 "2016-02-25 fix translation [OSTHUS]" ;
                 skos:definition "An electrophoresis method for the separation of amphoteric analytes according to their isoelectric points by the application of an electric field along a pH gradient. The pH gradient is formed during casting of the gels therefore cannot be altered during the experiment. [CHMO]" ;
                 skos:prefLabel "immobilised pH gradient-based isoelectric focusing"@en-gb ,
                                "immobilized pH gradient-based isoelectric focusing" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001922
af-p:AFP_0001922 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000365 ,
                                 af-p:AFP_0000474 ;
                 skos:altLabel "CIEF" ,
                               "capillary isoelectric focussing" ;
                 skos:definition "An electrophoresis method for the separation of amphoteric analytes according to their isolectric points by the application of an electric field along a pH gradient in a capillary. [CHMO]" ;
                 skos:prefLabel "capillary isoelectric focusing" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002035
af-p:AFP_0002035 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000474 ,
                                 af-p:AFP_0001644 ;
                 skos:altLabel "CITP" ;
                 skos:definition "An electrophoresis method in a capillary containing a discontinuous buffer system where the analytes migrate according to their electrophoretic mobilities, forming a chain of adjacent zones moving with equal velocity between two solutions, the leading and terminating electrolytes, bracketing the mobility range of the analytes. [CHMO]" ;
                 skos:prefLabel "capillary isotachophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002317
af-p:AFP_0002317 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001969 ;
                 skos:altLabel "2D CE" ,
                               "2D-CE" ,
                               "CE-CE" ,
                               "CE/CE" ;
                 skos:definition "A two-dimensional separation method where the separation processes are both capillary electrophoresis. [CHMO]" ;
                 skos:prefLabel "two-dimensional capillary electrophoresis" .


