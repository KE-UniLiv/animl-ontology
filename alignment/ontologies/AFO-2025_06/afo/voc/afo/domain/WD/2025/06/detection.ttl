af-p:AFP_0001223 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001909 ;
                 skos:definition "The detection of luminescence emitted from a sample as a result of the impact of high-energy electrons. [CHMO]" ;
                 skos:prefLabel "cathodoluminescence detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001314
af-p:AFP_0001314 rdf:type owl:Class ;
