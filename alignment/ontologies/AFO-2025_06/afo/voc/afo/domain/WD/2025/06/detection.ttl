af-p:AFP_0001009 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002095 ;
                 skos:altLabel "HDID" ;
                 skos:definition "A detection method where metastable helium ions (generated by an electric arc) ionize the molecules in the sample (held in He carrier gas). The resulting electrons are focused toward a collector electrode and the current is measured. [CHMO]" ;
                 skos:prefLabel "helium discharge ionisation detection"@en-gb ,
                                "helium discharge ionization detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001024
af-p:AFP_0001024 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001909 ;
                 skos:altLabel "EL detection" ;
                 skos:definition "The detection of luminescence emitted from a sample as a result of the application of an electric current or a strong electric field. [CHMO]" ;
                 skos:prefLabel "electroluminescence detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001029
