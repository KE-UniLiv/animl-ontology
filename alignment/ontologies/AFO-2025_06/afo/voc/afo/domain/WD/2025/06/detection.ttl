###  http://purl.allotrope.org/ontologies/equipment#AFE_0000046
af-e:AFE_0000046 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 chmo:_0002247 ;
                 skos:altLabel "DLATGS detector" ,
                               "DLaTGS detector" ,
                               "dLaTgS detector" ,
                               "deuterated L-alanine doped triglycine sulfate (DLaTGS) detector" ,
                               "deuterated lanthanum triglycine sulfate (DLATGS) detector " ,
                               "dlatgs detector" ;
                 skos:definition "A very sensitive temperature-controlled detector for mid-infrared range measurement that employs temperature-sensitive ferroelectric crystals of either deuterated lanthanum triglycine sulfate or deuterated L-alanine doped triglycine sulfate. As the temperature and hence polarizability of the crystals changes (due to the absorption of infrared radiation) a charge is generated which is detected by two parallel electrodes. The deuterated forms of the crystals are used because they have a higher Curie point. [CHMO]" ;
                 skos:prefLabel "deuterated lanthanum triglycine sulfate detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000059
af-e:AFE_0000059 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 <http://purl.obolibrary.org/obo/MS_1000114> ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "Wiza, J. (1979). \"Microchannel plate detectors\". Nuclear Instruments and Methods 162 (1–3): 587–601" ;
                 skos:altLabel "CEMA" ,
                               "channel electron multiplier array" ,
                               "multichannel plate" ;
                 skos:definition "Thin plate that contains a closely spaced array of channels each of which acts as a continuous dynode particle multiplier. A fast charged or neutral particle, or a photon, that strikes the plate causes a cascade of secondary electrons that ultimately exits the opposite side of the plate. [IUPAC]" ;
                 skos:prefLabel "microchannel plate detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000159
af-e:AFE_0000159 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 <http://purl.obolibrary.org/obo/MS_1000349> ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "F. Dubois, R. Knochenmuss, R. Zenobi. Int. J. Mass Spectrom. Ion Processes 169, 89 (1997)."@en-gb ;
                 skos:definition "Detector in which ions strike a conversion dynode to produce electrons that in turn strike a phosphor and the resulting photons are detected by a photomultiplier. [IUPAC]" ;
                 skos:prefLabel "ion-to-photon detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000166
af-e:AFE_0000166 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 chmo:_0002248 ;
                 skos:altLabel "MCT detector" ;
                 skos:definition "A detector for infrared measurements that employs a thin layer of the semiconductor HgCdTe. Incident photons with energy greater than the semiconductor band-gap energy excite electrons into the conduction band, creating an increase in the conductivity of the layer which is detected by two electrodes. [CHMO]" ;
                 skos:prefLabel "mercury cadmium tellurium detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000178
af-e:AFE_0000178 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000567 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "LIF" ;
                 skos:definition "A laser induced fluorescence detector detects species after excitation with a laser. The excited compounds deexcite after some time and emit light at a different wavelength which is recorded with photodiodes. [Allotrope]" ;
                 skos:prefLabel "laser induced fluorescence detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000206
af-e:AFE_0000206 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002808 <http://purl.obolibrary.org/obo/MS_1000624> ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "M. B. Comisarow. J. Chem. Phys. 69, 4097 (1978)." ,
                            "S. D. Fuerstenau, W. H. Benner. Rapid Commun. Mass Spectrom. 9, 1528 (1995)." ;
                 skos:altLabel "image current detector" ;
                 skos:definition "Method of ion detection by measuring the current that results from the image charge induced by a coherent motion of ions passing near a conductor. [IUPAC]" ;
                 skos:note "In Fourier transform mass spectrometers or Kingdon traps, the coherent orbital motion of ions between a pair of electrodes induces a radio frequency image charge in the electrodes and an alternating image current in the detection circuit connecting to the electrodes.  [IUPAC]" ;
                 skos:prefLabel "inductive detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000215
af-e:AFE_0000215 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 dct:source "Langmuir, Irving (1925). \"Thermionic Effects Caused by Vapours of Alkali Metals\". Proceedings of the Royal Society A 107: 61–79" ,
                            "Taylor, John (1930). \"The Reflection of Beams of the Alkali Metals from Crystals\". Physical Review 35 (4): 375–380" ;
                 skos:altLabel "surface ionisation detector"@en-gb ,
                               "hot wire detector" ,
                               "surface ionization detector" ;
                 skos:changeNote "2016-03-23 remove duplicate definition [Allotrope]" ;
                 skos:definition "A Langmuir–Taylor detector, also called surface ionization detector or hot wire detector, is a kind of detector developed by Taylor based on the work of Langmuir and Kingdon. This detector usually consists of a heated thin filament or ribbon of a metal with a high work function (typically tungsten or rhenium). Neutral atoms or molecules that strike the filament can boil off as positive ions in a process known as surface ionization, and these may be either measured as a current or detected, individually, using an electron multiplier and particle counting electronics. [Wikipedia]" ;
                 skos:prefLabel "Langmuir-Taylor detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000357
af-e:AFE_0000357 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 obi:_0000547 ;
                 skos:altLabel "ELSD" ;
                 skos:definition "A detector that uses the evaporative light scattering detection method. [Allotrope]" ;
                 skos:prefLabel "evaporative light scattering detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000419
af-e:AFE_0000419 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 obi:_0000588 ;
                 skos:altLabel "FPD" ,
                               "SCD" ,
                               "sulfur chemiluminescence detector" ;
                 skos:definition "A detector the uses the flame photometric detection method. [Allotrope]" ;
                 skos:prefLabel "flame photometric detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000444
af-e:AFE_0000444 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002809 <http://purl.obolibrary.org/obo/MS_1000348> ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "J. A. Hill, J. E. Biller, S. A. Martin, K. Biemann, K. Yoshidome, K. Sato. Int. J. Mass Spectrom. Ion Processes 92, 211 (1989)." ,
                            "K. K. Murray, R. K. Boyd, M. N. Eberlin, G. J. Langley, L. Li and Y. Naito, Pure Appl. Chem., 2013, 85, 1515-1609, 10.1351/PAC-REC-06-04-06" ;
                 skos:altLabel "focal plane collector" ;
                 skos:definition "Detector for m/z spatially dispersed ion beams in which all ions simultaneously impinge on the detector plane to produce spatially defined signals. [IUPAC]" ;
                 skos:prefLabel "focal plane detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000547
af-e:AFE_0000547 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 chmo:_0002532 ;
                 skos:altLabel "ET detector" ;
                 skos:definition "A detector used in scanning electron microscopy experiments to detect secondary electrons. The detector consists of a scintillator inside a Faraday cage inside the specimen chamber of the microscope. A low positive voltage is applied to the Faraday cage to attract the relatively low-energy (less than 50 eV) secondary electrons. [CHMO]" ;
                 skos:prefLabel "Everhart-Thornley detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000606
af-e:AFE_0000606 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 <http://purl.obolibrary.org/obo/MS_1000115> ;
                 skos:changeNote "2024-09-12 Replaced unlicensed definition with an Allotrope written one." ;
                 skos:definition "A multi collector is an ion detector using multiple collectors in series to detect ions simultaneously. [Allotrope]" ;
                 skos:prefLabel "multi-collector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000829
af-e:AFE_0000829 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000642 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 chmo:_0001063 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "W. K. Van Asselt, B. Poelsema, A. L. Boers. J. Phys. D: Appl. Phys. 11, L107 (1978)."@en-gb ;
                 skos:altLabel "Daly" ;
                 skos:definition "Detector consisting of an off-axis conversion dynode, scintillator, and photomultiplier. The dynode at high potential emits secondary electrons when ions impinge on the surface. The secondary electrons are accelerated onto the scintillator that produces light that is then detected by the photomultiplier detector, which can be outside the vacuum system. [IUPAC]" ;
                 skos:prefLabel "Daly detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000861
af-e:AFE_0000861 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 chmo:_0002246 ;
                 skos:altLabel "DGTS detector" ,
                               "DTGS detector" ,
                               "deuterated-triglycine sulfate (DTGS) detector" ;
                 skos:changeNote "2017-09-05 Linked to obo exact match [Allotrope]" ;
                 skos:definition "A very sensitive room-temperature detector for mid-infrared range measurements that employs temperature-sensitive ferroelectric crystals of deuterated triglycine sulfate. As the temperature and hence polarizability of the crystals changes (due to the absorption of infrared radiation) a charge is generated which is detected by two parallel electrodes. The deuterated form of the crystals are used because they have a higher Curie point. [CHMO]" ;
                 skos:prefLabel "deuterated triglycine sulfate detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001666
af-e:AFE_0001666 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002808 obi:_0001086 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "IUPAC. Compendium of Chemical Terminology, 2nd ed. (the \"Gold Book\"). Compiled by A. D. McNaught and A. Wilkinson. Blackwell Scientific Publications, Oxford (1997)." ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ,
                                 "2024-09-12 Changed [IUPAC Gold Book] attribution to [IUPAC] and added license, rights, and source fields." ;
                 skos:definition "A radiation detector using a medium in which a burst of luminescence radiation, produced along the path of an ionizing particle, is quantified. [IUPAC]" ;
                 skos:prefLabel "radiation counter" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001667
af-e:AFE_0001667 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001666 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002809 obi:_0001070 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "IUPAC. Compendium of Chemical Terminology, 2nd ed. (the \"Gold Book\"). Compiled by A. D. McNaught and A. Wilkinson. Blackwell Scientific Publications, Oxford (1997)." ;
                 skos:altLabel "scintillation counter" ,
                               "spot counter" ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ,
                                 "2024-09-12 Changed [IUPAC Gold Book] attribution to [IUPAC] and added license, rights, and source fields." ;
                 skos:definition "A scintillation detector is a scintillator that is coupled to a photomultiplier tube. Incident X-ray photons are converted in the scintillator into bursts of visible light photons, some of which fall on the photocathode and can be measured. For incident photons having energies higher than the absorption edge of the elements contained in the scintillator, an escape peak can be observed. [IUPAC]" ;
                 skos:prefLabel "scintillation detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001702
af-e:AFE_0001702 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002808 obi:_0000637 ;
                 skos:altLabel "air detector" ,
                               "air sensor" ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ,
                                 "2024-09-12 Changed [Amgen] attribution to [Allotrope]." ;
                 skos:definition "A gas detector is a device that detects the presence of gases (bubbles) in liquids. [Allotrope]" ;
                 skos:example "A sensor used to detect air (i.e. air bubbles) in the system flow path or sample flow path. Protects against air entering the pumps and columns and causing erratic flow, noisy UV detector signal and irregular pressure." ;
                 skos:prefLabel "gas detector" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001720
af-e:AFE_0001720 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000317 ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ;
                 skos:definition "A metal checker is a device that detects metal fragments. [Allotrope]" ;
                 skos:example "metal checker of a tablet press that rejects fragments of metal" ;
                 skos:prefLabel "metal checker" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001756
af-e:AFE_0001756 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001666 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "position sensitive detector" ,
                               "silicon strip detector" ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ;
                 skos:definition "A strip detector is a detector that is based on silicon strips which serve as charge collecting electrodes. [Allotrope]" ;
                 skos:prefLabel "strip detector" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000007
af-p:AFP_0000007 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000831 ;
                 skos:altLabel "argon pulse discharge photoionisation detection"@en-gb ,
                               "argon pulsed discharge photoionisation detection"@en-gb ,
                               "argon pulsed-discharge photoionisation detection"@en-gb ,
                               "Ar-PDPID" ,
                               "Ar/PDPID" ,
                               "argon pulse discharge photoionization detection" ,
                               "argon pulsed discharge photoionization detection" ,
                               "argon pulsed-discharge photoionization detection" ;
                 skos:definition "A detection method where a DC voltage pulse is passed through argon gas generating high-energy (13.5-17.5 eV) photons which photoionize the elements in the sample mixture, and the current generated by this ionization is measured. [CHMO]" ;
                 skos:prefLabel "argon pulse-discharge photoionisation detection"@en-gb ,
                                "argon pulse-discharge photoionization detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000027
af-p:AFP_0000027 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001711 ;
                 skos:altLabel "PED" ;
                 skos:definition "A detection method that is sensitive to compounds which can be either reduced or oxidised. A liquid containing the sample passes directly over the working electrode, which is subject to a periodic potential. The current produced is then measured. [CHMO]" ;
                 skos:prefLabel "pulsed electrochemical detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000088
af-p:AFP_0000088 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002222 ;
                 skos:altLabel "SCL detection" ,
                               "sono chemiluminescence detection" ,
                               "sono-chemiluminescence detection" ,
                               "sonodynamic chemiluminescence detection" ;
                 skos:definition "The detection of chemiluminescence caused the emission of light by tiny bubbles in a liquid excited by sound. [CHMO]" ;
                 skos:prefLabel "sonochemiluminescence detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000243
af-p:AFP_0000243 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000831 ;
                 skos:altLabel "krypton pulse discharge photoionisation detection"@en-gb ,
                               "krypton pulsed discharge photoionisation detection"@en-gb ,
                               "krypton pulsed-discharge photoionisation detection"@en-gb ,
                               "Kr-PDPID" ,
                               "Kr/PDPID" ,
                               "krypton pulse discharge photoionization detection" ,
                               "krypton pulsed discharge photoionization detection" ,
                               "krypton pulsed-discharge photoionization detection" ;
                 skos:definition "A detection method where a DC voltage pulse is passed through krypton gas generating high-energy (13.5-17.5 eV) photons which photoionize the elements in the sample mixture, and the current generated by this ionization is measured. [CHMO]" ;
                 skos:prefLabel "krypton pulse-discharge photoionisation detection"@en-gb ,
                                "krypton pulse-discharge photoionization detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000302
af-p:AFP_0000302 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000925 ;
                 skos:altLabel "PDD" ,
                               "pulse discharge detection" ,
                               "pulsed discharge detection" ,
                               "pulsed-discharge detection" ;
                 skos:definition "A detection method where a DC voltage pulse is passed through a carrier gas (e.g. helium) and the current generated by this ionization is measured. [CHMO]" ;
                 skos:prefLabel "pulse-discharge detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000327
af-p:AFP_0000327 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000925 ;
                 skos:altLabel "CCD" ;
                 skos:definition "A detection method that is sensitive to compounds containing C-H bonds. An electric current is passed through a tiny coil of platinum wire embedded in a catalytic ceramic bead (heated to 500 deg. C by the coil). A flow of air carries the sample to the bed and when a hydrogen or a hydrocarbon molecule impacts the hot coil, it combusts on the surface raising the temperature and resistance of the platinum wire. The resistance change is then measured. [CHMO]" ;
                 skos:prefLabel "catalytic combustion detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000352
af-p:AFP_0000352 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002222 ;
                 skos:altLabel "CIEEL " ,
                               "catalysed chemiluminescence" ,
                               "catalyzed chemiluminescence" ,
                               "chemically initiated electron exchange luminescence" ,
                               "chemically-initiated electron exchange luminescence" ;
                 skos:definition "The detection of luminescence resulting from a thermal electron-transfer reaction. [CHMO]" ;
                 skos:prefLabel "catalysed chemiluminescence detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000435
af-p:AFP_0000435 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000925 ;
                 skos:altLabel "AED" ,
                               "atomic-emission detection" ;
                 skos:definition "A detection method that is sensitive to individual elements. The sample is atomised and excited by an excitation source and the characteristic radiation emitted by the atoms in the sample is then detected using a photodiode array. [CHMO]" ;
                 skos:prefLabel "atomic emission detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000456
af-p:AFP_0000456 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001189 ;
                 skos:altLabel "DELCD" ;
                 skos:definition "A detection method that is sensitive to compounds containing Br or Cl. A carrier gas flow containing the sample is heated in a ceramic chamber to 1000 deg. C, forming gaseous Cl and Br ions. The change in electrical conductivity of the gas-phase current is then measured. [CHMO]" ;
                 skos:prefLabel "dry electrolytic conductivity detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000528
af-p:AFP_0000528 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001749 ;
                 skos:altLabel "TC-SPC" ,
                               "TCSPC" ,
                               "time correlated single photon counting" ;
                 skos:definition "A method that is used to measure the relaxation rate of molecules in fluorescent samples. A laser pulse excites the molecule, and the time taken for it to emit a single photon is measured. [CHMO]" ;
                 skos:prefLabel "time-correlated single photon counting" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000579
af-p:AFP_0000579 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001909 ;
                 skos:altLabel "RET" ;
                 skos:definition "Any method where a biological sample is illuminated with a light source and the energy transfer between molecules can be used for imaging and to measure inter-molecular distances. [CHMO]" ;
                 skos:prefLabel "resonance energy transfer" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000603
af-p:AFP_0000603 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001009 ;
                 skos:altLabel "pulse-discharge helium ionisation detection"@en-gb ,
                               "PDHID" ,
                               "pulse-discharge helium ionization detection" ;
                 skos:definition "A detection method that is sensitive to small gas molecules (e.g. O2, N2, CO2). A low-power, pulsed DC discharge is used to ionize the sample (which is held in He carrier gas). The resulting electrons are focused toward a collector electrode and the current is measured. [CHMO]" ;
                 skos:prefLabel "pulsed discharge helium ionisation detection"@en-gb ,
                                "pulsed discharge helium ionization detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000831
af-p:AFP_0000831 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000925 ;
                 skos:altLabel "pulse discharge photoionisation detection"@en-gb ,
                               "pulsed discharge photoionisation detection"@en-gb ,
                               "pulsed-discharge photoionisation detection"@en-gb ,
                               "PDPID" ,
                               "pulse discharge photoionization detection" ,
                               "pulsed discharge photoionization detection" ,
                               "pulses-discharge photoionization detection" ;
                 skos:definition "A detection method where a DC voltage pulse is passed through a carrier gas generating high-energy (13.5-17.5 eV) photons which photoionize the elements in the sample mixture, and the current generated by this ionization is measured. [CHMO]" ;
                 skos:prefLabel "pulse-discharge photoionisation detection"@en-gb ,
                                "pulse-discharge photoionization detection" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000888
af-p:AFP_0000888 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000925 ;
