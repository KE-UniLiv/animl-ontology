                 rdfs:subClassOf obi:_0000011 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty af-x:AFX_0000352 ;
                                   owl:someValuesFrom af-p:AFP_0001427
                                 ] ;
