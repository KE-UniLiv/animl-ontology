                 dct:source "K. K. Murray, R. K. Boyd, M. N. Eberlin, G. J. Langley, L. Li and Y. Naito, Pure Appl. Chem., 2013, 85, 1515-1609" ;
                 skos:altLabel "TLF" ;
                 skos:definition "Energy focusing of ions formed in the gas phase in a time-of-flight mass spectrometer that is accomplished by introducing a time delay between the formation of the ions and the application of the accelerating potential pulse. [IUPAC]" ;
                 skos:prefLabel "time lag focusing" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002098
af-p:AFP_0002098 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001427 ;
                 skos:altLabel "ambient mass spectrometry" ;
                 skos:definition "Any mass spectrometry technique where the sample is ionized at low or ambient temperature. [CHMO]" ;
                 skos:prefLabel "ambient ionization mass spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002107
af-p:AFP_0002107 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001427 ;
                 dct:source "K. K. Murray, R. K. Boyd, M. N. Eberlin, G. J. Langley, L. Li and Y. Naito, Pure Appl. Chem., 2013, 85, 1515-1609" ;
                 skos:altLabel "IKES" ;
                 skos:definition "A method of analysis in which a beam of ions is separated according to the ratio of its translational energy to charge. [PSI-MS]" ;
                 skos:prefLabel "ion kinetic energy spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002148
af-p:AFP_0002148 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001427 ;
                 skos:altLabel "PS-MS" ,
                               "plasma source mass spectroscopy" ;
                 skos:definition "Any type of mass spectrometry where the sample is ionized in a plasma (a partially ionized gas - such as Ar - containing free electrons). [CHMO]" ;
                 skos:prefLabel "plasma mass spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002156
af-p:AFP_0002156 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001577 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "K. K. Murray, R. K. Boyd, M. N. Eberlin, G. J. Langley, L. Li and Y. Naito, Pure Appl. Chem., 2013, 85, 1515-1609" ;
                 skos:altLabel "ID-MS" ,
                               "IDMS" ,
                               "isotope dilution mass spectroscopy" ;
                 skos:definition "Quantitative mass spectrometry technique in which an isotopically enriched compound is used as a surrogate internal standard. [IUPAC]" ;
                 skos:prefLabel "isotope dilution mass spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002182
af-p:AFP_0002182 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002198 ;
                 skos:altLabel "MALDI-IMS" ,
                               "MALDI-MS imaging" ,
                               "matrix-assisted laser desorption-ionization imaging mass spectroscopy" ;
                 skos:definition "The collection of spatially resolved mass spectra of a sample during microscopy where a pulse laser is used to produce both ionization and vapourization and the sample is held in a matrix (such as dihydrobenzoic acid) to protect it from direct laser illumination and to separate the sample molecules. The spectra are used to visualise the spatial distribution of (typically) biomolecules by their molecular masses. [CHMO]" ;
                 skos:prefLabel "matrix-assisted laser desorption-ionization imaging mass spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002183
af-p:AFP_0002183 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001026 ,
                                 af-p:AFP_0001171 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "K. K. Murray, R. K. Boyd, M. N. Eberlin, G. J. Langley, L. Li and Y. Naito, Pure Appl. Chem., 2013, 85, 1515-1609" ;
                 skos:altLabel "DART" ;
                 skos:definition "An ionization method where the sample is ionized by a corona discharge within a He atmosphere. [CHMO]" ;
                 skos:note "Direct analysis in real time is a proprietary term indicating the formation of ions from a solid or liquid sample at atmospheric pressure through the interaction of a gas stream containing internally excited atoms or molecules with the surface. The excited-state species are typically formed in a glow discharge in helium or nitrogen. The term should only be used to describe the commercial product. [IUPAC]" ;
                 skos:prefLabel "direct analysis in real time" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002186
af-p:AFP_0002186 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000108 ;
                 skos:altLabel "PTR-MS" ,
                               "PTR/MS" ,
                               "PTRMS" ,
                               "proton transfer reaction mass spectroscopy" ;
                 skos:definition "A type of mass spectrometry where the sample is ionized by introducing it into a tube containing fast flowing He gas and H3O+ ions. [CHMO]" ;
                 skos:prefLabel "proton transfer reaction mass spectrometry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002187
af-p:AFP_0002187 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001769 ;
                 skos:altLabel "PQD" ;
                 skos:definition "A process that involves precursor ion activation at high Q, a time delay to allow the precursor to fragment, then a rapid pulse to low Q where all fragment ions are trapped. The product ions can then be scanned out of the ion trap and detected. [PSI-MS]" ;
                 skos:prefLabel "pulsed q dissociation" .


