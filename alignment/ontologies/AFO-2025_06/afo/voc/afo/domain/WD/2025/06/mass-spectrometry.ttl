                 skos:definition "Ejection of an ion from an ion trap in a direction parallel to the longitudinal axis of the trap. [IUPAC]" ;
                 skos:prefLabel "axial ejection" .


###  http://purl.allotrope.org/ontologies/result#AFR_0000003
af-r:AFR_0000003 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0000372 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
