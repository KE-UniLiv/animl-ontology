###  http://purl.allotrope.org/ontologies/equipment#AFE_0001475
af-e:AFE_0001475 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001648 ;
                 dct:source "https://en.wikipedia.org/wiki/Closure_%28container%29" ;
                 skos:changeNote "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "Crown caps are shallow metal caps that are crimped into locking position around the head of the bottle. [Wikipedia]" ;
                 skos:example "Beverage bottles are frequently closed with crown caps." ;
                 skos:prefLabel "crown cap" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001478
af-e:AFE_0001478 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001648 ;
                 dct:source "https://en.wikipedia.org/wiki/Closure_%28container%29" ;
                 skos:altLabel "C-R cap" ,
                               "safety cap" ;
                 skos:changeNote "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "Child-resistant packaging has special closures designed to reduce the risk of children ingesting dangerous items. This is often accomplished by the use of a special safety cap. [Wikipedia]" ;
                 skos:prefLabel "child-resistant cap" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001494
af-e:AFE_0001494 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001493 ;
                 dct:source "https://en.wikipedia.org/wiki/Bung" ,
                            "https://en.wikipedia.org/wiki/Ground_glass_joint" ;
                 skos:altLabel "glass stopper" ;
                 skos:changeNote "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "Ground glass joint stoppers are stoppers commonly used with laboratory glassware, mainly because of their nonreactivity. Some stoppers used in labs have holes in them to allow the insertion of glass or rubber tubing. [Wikipedia]" ;
                 skos:prefLabel "ground glass stopper" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001495
af-e:AFE_0001495 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001493 ;
                 dct:source "https://en.wikipedia.org/wiki/Bung" ;
                 skos:altLabel "rubber stopper" ;
                 skos:changeNote "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "A stopper made of hardened rubber. [Allotrope]" ;
                 skos:prefLabel "rubber bung" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001496
af-e:AFE_0001496 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001493 ;
                 dct:source "https://en.wikipedia.org/wiki/Bung" ;
                 skos:altLabel "cork" ;
                 skos:changeNote "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "A stopper made of cork. [Allotrope]" ;
                 skos:prefLabel "cork stopper" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001697
af-e:AFE_0001697 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001648 ;
                 skos:changeNote "2015-11-25 add concept [Amgen]" ,
                                 "2017-10-24 moved to closure domain ontology [OSTHUS]" ;
                 skos:definition "An aluminum lid is a lid that is made of aluminum. [Allotrope]" ;
                 skos:prefLabel "aluminum lid" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001698
