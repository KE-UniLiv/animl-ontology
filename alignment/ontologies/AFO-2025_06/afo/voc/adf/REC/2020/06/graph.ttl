@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix adf-g: <http://purl.allotrope.org/ontologies/graph#> .
@base <http://purl.allotrope.org/voc/adf/REC/2020/06/graph#> .

<http://purl.allotrope.org/voc/adf/REC/2020/06/graph> rdf:type owl:Ontology ;
                                                       owl:versionIRI <http://purl.allotrope.org/voc/adf/REC/2020/06/graph> ;
                                                       dct:contributor <http://www.allotrope.org> ;
                                                       dct:description "Ontology for describing graphs in ADF. (REC/2020/06)" ;
                                                       dct:issued "2020-06-30T12:00:00Z"^^xsd:dateTime ;
                                                       dct:license <http://purl.allotrope.org/voc/creative-commons-attribution-license> ;
