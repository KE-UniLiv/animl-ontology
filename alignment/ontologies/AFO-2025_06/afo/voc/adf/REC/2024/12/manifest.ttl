###  http://purl.allotrope.org/ontologies/manifest#json-schemas
af-mf:json-schemas rdf:type owl:AnnotationProperty ;
                   skos:definition "This annotation on the manifest refers to a JSON schema for an Allotrope Simple Model (ASM). [Allotrope]" ;
                   skos:prefLabel "JSON schemas" .


###  http://purl.allotrope.org/ontologies/manifest#shapes
af-mf:shapes rdf:type owl:AnnotationProperty ;
             skos:definition "This annotation on the manifest refers to a data model (shape library) that should be used with the data description. It is used with together with owl:imports. [Allotrope]" ;
             skos:prefLabel "shapes" .


###  http://purl.allotrope.org/ontologies/manifest#vocabulary
af-mf:vocabulary rdf:type owl:AnnotationProperty ;
                 skos:definition "This annotation on the manifest refers to a vocabulary (ontologies, taxonomies) that should be used with the data description. It is used with together with owl:imports. [Allotrope]" ;
                 skos:prefLabel "vocabulary" .


