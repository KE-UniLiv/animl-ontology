#################################################################
#    Object Properties
#################################################################

###  http://qudt.org/schema/qudt#allowedUnitOfSystem
qudt:allowedUnitOfSystem rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf qudt:unitOfSystem ;
                         dct:description "This property relates a unit of measure with a unit system that does not define the unit, but allows its use within the system. An allowed unit must be convertible to some dimensionally eqiuvalent unit that is defined by the system." ;
                         rdfs:label "allowed unit of system" ;
                         skos:definition "This property relates a unit of measure with a unit system that does not define the unit, but allows its use within the system. An allowed unit must be convertible to some dimensionally eqiuvalent unit that is defined by the system." ;
                         skos:prefLabel "allowed unit of system" .


###  http://qudt.org/schema/qudt#baseDimensionEnumeration
qudt:baseDimensionEnumeration rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              dct:description "This property associates a system of quantities with an enumeration that enumerates the base dimensions of the system in canonical order." ;
                              rdfs:label "base dimension enumeration" ;
                              skos:definition "This property associates a system of quantities with an enumeration that enumerates the base dimensions of the system in canonical order." ;
                              skos:prefLabel "base dimension enumeration" .


###  http://qudt.org/schema/qudt#baseQuantityKindOfSystem
qudt:baseQuantityKindOfSystem rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf qudt:quantityKindOfSystem ;
                              owl:inverseOf qudt:systemBaseQuantityKind ;
                              rdfs:label "base quantity kind of system" ;
                              skos:prefLabel "base quantity kind of system" .


###  http://qudt.org/schema/qudt#baseUnitOfSystem
qudt:baseUnitOfSystem rdf:type owl:ObjectProperty ;
                      owl:inverseOf qudt:systemBaseUnit ;
                      dct:description "This property relates a unit of measure to the system of units in which it is defined as a base unit for the system. The base units of a system are used to define the derived units of the system by expressing the derived units as products of the base units raised to a rational power." ;
                      rdfs:label "base unit of system" ;
                      skos:definition "This property relates a unit of measure to the system of units in which it is defined as a base unit for the system. The base units of a system are used to define the derived units of the system by expressing the derived units as products of the base units raised to a rational power." ;
                      skos:prefLabel "base unit of system" .


###  http://qudt.org/schema/qudt#basisElement
qudt:basisElement rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:label "basis element" ;
                  skos:altLabel "basis element" ;
                  skos:prefLabel "has basis element" .


###  http://qudt.org/schema/qudt#coherentDerivedUnitOfSystem
qudt:coherentDerivedUnitOfSystem rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf qudt:derivedUnitOfSystem ;
                                 owl:inverseOf qudt:systemCoherentDerivedUnit ;
                                 dct:description "This property relates a unit of measure to the unit system in which the unit is derived from the system's base units with a proportionality constant of one." ;
                                 rdfs:label "derived unit of system" ;
                                 skos:altLabel "derived unit of system" ;
                                 skos:definition "This property relates a unit of measure to the unit system in which the unit is derived from the system's base units with a proportionality constant of one." ;
                                 skos:prefLabel "has derived unit of system" .


###  http://qudt.org/schema/qudt#coherentUnitSystem
qudt:coherentUnitSystem rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf qudt:unitSystem ;
                        rdf:type owl:FunctionalProperty ;
                        dct:description "A system of units is coherent with respect to a system of quantities and equations if the system of units is chosen in such a way that the equations between numerical values have exactly the same form (including the numerical factors) as the corresponding equations between the quantities. In such a coherent system, no numerical factor other than the number 1 ever occurs in the expressions for the derived units in terms of the base units." ;
                        rdfs:label "coherent unit system" ;
                        skos:altLabel "coherent unit system" ;
                        skos:definition "A system of units is coherent with respect to a system of quantities and equations if the system of units is chosen in such a way that the equations between numerical values have exactly the same form (including the numerical factors) as the corresponding equations between the quantities. In such a coherent system, no numerical factor other than the number 1 ever occurs in the expressions for the derived units in terms of the base units." ;
                        skos:prefLabel "has coherent unit system" .


###  http://qudt.org/schema/qudt#default
qudt:default rdf:type owl:ObjectProperty ;
             dct:description "The default element in an enumeration" ;
             rdfs:label "default" ;
             skos:altLabel "default" ;
             skos:definition "The default element in an enumeration" ;
             skos:prefLabel "has default" .


###  http://qudt.org/schema/qudt#definedUnitOfSystem
qudt:definedUnitOfSystem rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf qudt:unitOfSystem ;
                         owl:inverseOf qudt:systemDefinedUnit ;
                         dct:description "This property relates a unit of measure with the unit system that defines the unit." ;
                         rdfs:label "defined unit of system" ;
                         skos:altLabel "defined unit of system" ;
                         skos:definition "This property relates a unit of measure with the unit system that defines the unit." ;
                         skos:prefLabel "has defined unit of system" .


###  http://qudt.org/schema/qudt#derivedQuantityKindOfSystem
qudt:derivedQuantityKindOfSystem rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf qudt:quantityKindOfSystem ;
                                 owl:inverseOf qudt:systemDerivedQuantityKind ;
                                 rdfs:label "derived quantity kind of system" ;
                                 skos:prefLabel "derived quantity kind of system" .


###  http://qudt.org/schema/qudt#derivedUnitOfSystem
qudt:derivedUnitOfSystem rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf qudt:definedUnitOfSystem ;
                         owl:inverseOf qudt:systemDerivedUnit ;
                         dct:description "This property relates a unit of measure to the system of units in which it is defined as a derived unit. That is, the derived unit is defined as a product of the base units for the system raised to some rational power." ;
                         rdfs:label "derived unit of system" ;
                         skos:definition "This property relates a unit of measure to the system of units in which it is defined as a derived unit. That is, the derived unit is defined as a product of the base units for the system raised to some rational power." ;
                         skos:prefLabel "derived unit of system" .


###  http://qudt.org/schema/qudt#dimensionInverse
qudt:dimensionInverse rdf:type owl:ObjectProperty ;
                      owl:inverseOf qudt:dimensionInverse ;
                      rdf:type owl:FunctionalProperty ,
                               owl:InverseFunctionalProperty ,
                               owl:SymmetricProperty ;
                      rdfs:label "dimension inverse" ;
                      skos:prefLabel "dimension inverse" .


###  http://qudt.org/schema/qudt#element
qudt:element rdf:type owl:ObjectProperty ;
             dct:description "An element of an enumeration" ;
             rdfs:label "element" ;
             skos:altLabel "element" ;
             skos:definition "An element of an enumeration" ;
             skos:prefLabel "has element" .


###  http://qudt.org/schema/qudt#elementKind
qudt:elementKind rdf:type owl:ObjectProperty ;
                 rdfs:label "element kind" ;
                 skos:altLabel "element kind" ;
                 skos:prefLabel "has element kind" .


###  http://qudt.org/schema/qudt#exactMatch
qudt:exactMatch rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                rdfs:label "exact match" ;
                skos:prefLabel "exact match" .


###  http://qudt.org/schema/qudt#generalization
qudt:generalization rdf:type owl:ObjectProperty ;
                    owl:inverseOf qudt:specialization ;
                    dct:description """This property relates a quantity kind to its generalization. A quantity kind, PARENT, is a generalization of the quantity kind CHILD only if

1. PARENT and CHILD have the same dimensions in every system of quantities;
2. Every unit that is a measure of quantities of kind CHILD is also a valid measure of quantities of kind PARENT.""" ;
                    rdfs:label "generalization" ;
                    skos:altLabel "generalization" ;
                    skos:definition """This property relates a quantity kind to its generalization. A quantity kind, PARENT, is a generalization of the quantity kind CHILD only if

1. PARENT and CHILD have the same dimensions in every system of quantities;
2. Every unit that is a measure of quantities of kind CHILD is also a valid measure of quantities of kind PARENT.""" ;
                    skos:prefLabel "has generalization" .


###  http://qudt.org/schema/qudt#quantity
qudt:quantity rdf:type owl:ObjectProperty ;
              dct:description "a property to relate an observable thing with a quantity (qudt:Quantity)" ;
              rdfs:label "quantity" ;
              skos:altLabel "quantity" ;
              skos:definition "a property to relate an observable thing with a quantity (qudt:Quantity)" ;
              skos:prefLabel "has quantity" .


###  http://qudt.org/schema/qudt#quantityKind
qudt:quantityKind rdf:type owl:ObjectProperty ;
                  rdfs:label "quantity kind" ;
                  skos:altLabel "quantity kind" ;
                  skos:prefLabel "has quantity kind" .


###  http://qudt.org/schema/qudt#quantityKindOfSystem
qudt:quantityKindOfSystem rdf:type owl:ObjectProperty ;
                          owl:inverseOf qudt:systemQuantityKind ;
                          rdfs:label "quantity kind of system" ;
                          skos:definition "inverse of system quantity kind" ;
                          skos:prefLabel "quantity kind of system" .


###  http://qudt.org/schema/qudt#quantityValue
qudt:quantityValue rdf:type owl:ObjectProperty ;
                   owl:inverseOf qudt:valueQuantity ;
                   rdfs:label "quantity value" ;
                   skos:altLabel "quantity value" ;
                   skos:definition "inverse of value quantity" ;
                   skos:prefLabel "has quantity value" .


###  http://qudt.org/schema/qudt#referenceThing
qudt:referenceThing rdf:type owl:ObjectProperty ;
                    rdfs:label "reference thing" ;
                    skos:altLabel "reference thing" ;
                    skos:prefLabel "has reference thing" .


###  http://qudt.org/schema/qudt#referenceUnit
qudt:referenceUnit rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf qudt:referenceThing ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:label "reference unit" ;
                   skos:altLabel "reference unit" ;
                   skos:prefLabel "has reference unit" .


###  http://qudt.org/schema/qudt#specialization
qudt:specialization rdf:type owl:ObjectProperty ;
                    dct:description "This property relates a quantity kind to its specialization(s). For example, linear velocity and angular velocity are both specializations of velocity." ;
                    rdfs:label "specialization" ;
                    skos:altLabel "specialization" ;
                    skos:definition "This property relates a quantity kind to its specialization(s). For example, linear velocity and angular velocity are both specializations of velocity." ;
                    skos:prefLabel "has specialization" .


###  http://qudt.org/schema/qudt#systemBaseQuantityKind
qudt:systemBaseQuantityKind rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf qudt:systemQuantityKind ;
                            rdfs:label "system base quantity kind" ;
                            skos:altLabel "system base quantity kind" ;
                            skos:prefLabel "has system base quantity kind" .


###  http://qudt.org/schema/qudt#systemBaseUnit
qudt:systemBaseUnit rdf:type owl:ObjectProperty ;
                    dct:description "This property relates a system of units to a base unit defined within the system. The base units of a system are used to define the derived units of the system by expressing the derived units as products of the base units raised to a rational power." ;
                    rdfs:label "base unit" ;
                    skos:altLabel "base unit" ;
                    skos:definition "This property relates a system of units to a base unit defined within the system. The base units of a system are used to define the derived units of the system by expressing the derived units as products of the base units raised to a rational power." ;
                    skos:prefLabel "has base unit" .


###  http://qudt.org/schema/qudt#systemCoherentDerivedUnit
qudt:systemCoherentDerivedUnit rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf qudt:systemDerivedUnit ;
                               rdfs:label "coherent derived unit" ;
                               skos:altLabel "coherent derived unit" ;
                               skos:prefLabel "has coherent derived unit" .


###  http://qudt.org/schema/qudt#systemDefinedUnit
qudt:systemDefinedUnit rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf qudt:systemUnit ;
                       dct:description "This property relates a unit system with a unit of measure that is defined by the system." ;
                       rdfs:label "defined unit" ;
                       skos:altLabel "defined unit" ;
                       skos:definition "This property relates a unit system with a unit of measure that is defined by the system." ;
                       skos:prefLabel "has defined unit" .


###  http://qudt.org/schema/qudt#systemDerivedQuantityKind
qudt:systemDerivedQuantityKind rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf qudt:systemQuantityKind ;
                               rdfs:label "system derived quantity kind" ;
                               skos:altLabel "system derived quantity kind" ;
                               skos:prefLabel "has system derived quantity kind" .


###  http://qudt.org/schema/qudt#systemDerivedUnit
qudt:systemDerivedUnit rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf qudt:systemDefinedUnit ;
                       dct:description "This property relates a system of units to a unit of measure that is defined within the system in terms of the base units for the system. That is, the derived unit is defined as a product of the base units for the system raised to some rational power." ;
                       rdfs:label "derived unit" ;
                       skos:altLabel "derived unit" ;
                       skos:definition "This property relates a system of units to a unit of measure that is defined within the system in terms of the base units for the system. That is, the derived unit is defined as a product of the base units for the system raised to some rational power." ;
                       skos:prefLabel "has derived unit" .


###  http://qudt.org/schema/qudt#systemDimension
qudt:systemDimension rdf:type owl:ObjectProperty ;
                     rdfs:label "system dimension" ;
                     skos:altLabel "system dimension" ;
                     skos:prefLabel "has system dimension" .


###  http://qudt.org/schema/qudt#systemQuantityKind
qudt:systemQuantityKind rdf:type owl:ObjectProperty ;
                        rdfs:label "system quantity kind" ;
                        skos:altLabel "system quantity kind" ;
                        skos:prefLabel "has system quantity kind" .


###  http://qudt.org/schema/qudt#systemUnit
qudt:systemUnit rdf:type owl:ObjectProperty ;
                owl:inverseOf qudt:unitOfSystem ;
                dct:description "This property relates a system of units with a unit of measure that is either a) defined by the system, or b) accepted for use by the system and is convertible to a unit of equivalent dimension that is defined by the system. Systems of units may distinguish between base and derived units. Base units are the units which measure the base quantities for the corresponding system of quantities. The base units are used to define units for all other quantities as products of powers of the base units. Such units are called derived units for the system." ;
                rdfs:label "system unit" ;
                skos:altLabel "system unit" ;
                skos:definition "This property relates a system of units with a unit of measure that is either a) defined by the system, or b) accepted for use by the system and is convertible to a unit of equivalent dimension that is defined by the system. Systems of units may distinguish between base and derived units. Base units are the units which measure the base quantities for the corresponding system of quantities. The base units are used to define units for all other quantities as products of powers of the base units. Such units are called derived units for the system." ;
                skos:prefLabel "has system unit" .


###  http://qudt.org/schema/qudt#unit
qudt:unit rdf:type owl:ObjectProperty ;
          owl:inverseOf qudt:unitFor ;
          rdfs:range qudt:Unit ;
          dct:description "A reference to the unit of measure of a quantity (variable or constant) of interest." ;
          rdfs:label "unit" ;
          skos:altLabel "unit" ;
          skos:definition "A reference to the unit of measure of a quantity (variable or constant) of interest." ;
          skos:prefLabel "has unit" .


###  http://qudt.org/schema/qudt#unitFor
qudt:unitFor rdf:type owl:ObjectProperty ;
             rdfs:label "unit for" ;
             skos:altLabel "unit of" ;
             skos:definition "This property relates a unit of measure to something." ;
             skos:prefLabel "unit for" .


###  http://qudt.org/schema/qudt#unitOfSystem
qudt:unitOfSystem rdf:type owl:ObjectProperty ;
                  dct:description "This property relates a unit of measure with a system of units that either a) defines the unit or b) allows the unit to be used within the system." ;
                  rdfs:label "unit of system" ;
                  skos:definition "This property relates a unit of measure with a system of units that either a) defines the unit or b) allows the unit to be used within the system." ;
                  skos:prefLabel "unit of system" .


###  http://qudt.org/schema/qudt#unitSystem
qudt:unitSystem rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:label "unit system" ;
                skos:altLabel "unit system" ;
                skos:prefLabel "has unit system" .


###  http://qudt.org/schema/qudt#value
qudt:value rdf:type owl:ObjectProperty ;
           dct:description "A property to relate an observable thing with a quantity value (qudt:QuantityValue)" ;
           rdfs:label "value" ;
           skos:altLabel "has value" ,
                         "value" ;
           skos:definition "A property to relate an observable thing with a quantity value (qudt:QuantityValue)" ;
           skos:prefLabel "has value" .


###  http://qudt.org/schema/qudt#valueQuantity
qudt:valueQuantity rdf:type owl:ObjectProperty ;
                   rdfs:label "value for quantity" ;
                   skos:prefLabel "value for quantity" .


#################################################################
#    Data properties
#################################################################

###  http://qudt.org/schema/qudt#abbreviation
qudt:abbreviation rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string ;
                  dct:description "An abbreviation for a unit is a short (usually 5 characters or less) string that is used in place of the full name for the unit in contexts where space is limited, or where using the abbreviation will enhance readability." ;
                  rdfs:label "abbreviation" ;
                  skos:altLabel "abbreviation" ;
                  skos:definition "An abbreviation for a unit is a short (usually 5 characters or less) string that is used in place of the full name for the unit in contexts where space is limited, or where using the abbreviation will enhance readability." ;
                  skos:prefLabel "has abbreviation" .


###  http://qudt.org/schema/qudt#code
qudt:code rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          dct:description "A unit code is a numeric string that uniquely identifies a unit " ;
          rdfs:label "code" ;
          skos:altLabel "code" ;
          skos:definition "A unit code is a numeric string that uniquely identifies a unit " ;
          skos:prefLabel "has code" .


###  http://qudt.org/schema/qudt#conversionCoefficient
qudt:conversionCoefficient rdf:type owl:DatatypeProperty ;
                           rdfs:range xsd:double ;
                           rdfs:label "conversion coefficient" ;
                           skos:altLabel "conversion coefficient" ;
                           skos:prefLabel "has conversion coefficient" .


###  http://qudt.org/schema/qudt#conversionMultiplier
qudt:conversionMultiplier rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:range xsd:double ;
                          rdfs:label "conversion multiplier" ;
                          skos:altLabel "conversion multiplier" ;
                          skos:prefLabel "has conversion multiplier" .


###  http://qudt.org/schema/qudt#conversionOffset
qudt:conversionOffset rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:range xsd:double ;
                      rdfs:label "conversion offset" ;
                      skos:altLabel "conversion offset" ;
                      skos:prefLabel "has conversion offset" .


###  http://qudt.org/schema/qudt#currencyExponent
qudt:currencyExponent rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:range xsd:integer ;
                      dct:description "The currency exponent indicates the number of decimal places between a major currency unit and its minor currency unit. For example, the US dollar is the major currency unit of the United States, and the US cent is the minor currency unit. Since one cent is 1/100 of a dollar, the US dollar has a currency exponent of 2. However, the Japanese Yen has no minor currency units, so the yen has a currency exponent of 0." ;
                      rdfs:label "currency exponent" ;
                      skos:altLabel "currency exponent" ;
                      skos:definition "The currency exponent indicates the number of decimal places between a major currency unit and its minor currency unit. For example, the US dollar is the major currency unit of the United States, and the US cent is the minor currency unit. Since one cent is 1/100 of a dollar, the US dollar has a currency exponent of 2. However, the Japanese Yen has no minor currency units, so the yen has a currency exponent of 0." ;
                      skos:prefLabel "has currency exponent" .


###  http://qudt.org/schema/qudt#description
qudt:description rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string ;
                 rdfs:label "description" ;
                 skos:altLabel "description" ;
                 skos:prefLabel "has description" .


###  http://qudt.org/schema/qudt#exactConstant
qudt:exactConstant rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:boolean ;
                   rdfs:label "exact constant" ;
                   skos:altLabel "exact constant" ;
                   skos:prefLabel "is exact constant" .


###  http://qudt.org/schema/qudt#literal
qudt:literal rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:label "literal" ;
             skos:altLabel "literal" ;
             skos:prefLabel "has literal" .


###  http://qudt.org/schema/qudt#negativeDeltaLimit
qudt:negativeDeltaLimit rdf:type owl:DatatypeProperty ;
                        rdfs:range xsd:string ;
                        dct:description "A negative change limit between consecutive sample values for a parameter. The Negative Delta may be the encoded value or engineering units value depending on whether or not a Calibrator is defined." ;
                        rdfs:label "negative delta limit" ;
                        skos:altLabel "negative delta limit" ;
                        skos:definition "A negative change limit between consecutive sample values for a parameter. The Negative Delta may be the encoded value or engineering units value depending on whether or not a Calibrator is defined." ;
                        skos:prefLabel "has negative delta limit" .


###  http://qudt.org/schema/qudt#numericValue
qudt:numericValue rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:double ;
                  rdfs:label "numeric value" ;
                  skos:altLabel "numeric value" ;
                  skos:prefLabel "has numeric value" .


###  http://qudt.org/schema/qudt#order
