###  http://qudt.org/schema/qudt#Dimension
qudt:Dimension rdf:type owl:Class ;
               rdfs:subClassOf qudt:EnumeratedValue ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty qudt:dimensionVector ;
                                 owl:allValuesFrom qudt:DimensionVector
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty qudt:referenceQuantity ;
                                 owl:allValuesFrom qudt:QuantityKind
                               ] ;
               dct:description "A dimension is a relationship between a quantity system, a quantity kind of that system, and one or more dimension vectors. There is one dimension vector for each of the system's base quantity kinds. The vector's magnitude determines the exponent of the base dimension for the referenced quantity kind." ;
               rdfs:label "Dimension" ;
               skos:definition "A dimension is a relationship between a quantity system, a quantity kind of that system, and one or more dimension vectors. There is one dimension vector for each of the system's base quantity kinds. The vector's magnitude determines the exponent of the base dimension for the referenced quantity kind." ;
               skos:prefLabel "dimension" .


###  http://qudt.org/schema/qudt#DimensionVector
qudt:DimensionVector rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty qudt:basisElement ;
                                       owl:allValuesFrom qudt:QuantityKind
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty qudt:basisElement ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass qudt:QuantityKind
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty qudt:vectorMagnitude ;
                                       owl:allValuesFrom xsd:float
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty qudt:vectorMagnitude ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:float
                                     ] ;
                     dct:description "A dimension vector is an association between a quantity kind and a rational number. The quantity kind serves as the basis vector in an abstract vector space, and the rational number is the vector magnitude. The abstract vector space is determined by the chosen set of base quantity kinds for a quantity system." ;
                     rdfs:label "Dimension Vector" ;
                     skos:definition "A dimension vector is an association between a quantity kind and a rational number. The quantity kind serves as the basis vector in an abstract vector space, and the rational number is the vector magnitude. The abstract vector space is determined by the chosen set of base quantity kinds for a quantity system." ;
                     skos:prefLabel "dimension vector" .


