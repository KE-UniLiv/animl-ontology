###  http://purl.allotrope.org/ontologies/property#AFX_0000001
af-x:AFX_0000001 rdf:type owl:ObjectProperty ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "Green Book, 2nd ed.: IUPAC Quantities, Units and Symbols in Physical Chemistry. Second Edition, Blackwell Scientific Publications, Oxford, 1993" ,
                            "PAC, 1996, 68, 957 (Glossary of terms in quantities and units in Clinical Chemistry (IUPAC-IFCC Recommendations 1996))" ;
                 skos:changeNote "2024-09-12 Added license and rights fields." ;
                 skos:definition "Normal force acting on a surface divided by the area of that surface. [IUPAC]" ;
                 skos:example "The pressure of a gas equals to the force that it exerts on the walls of its container." ;
                 skos:prefLabel "pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000002
af-x:AFX_0000002 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "Green Book, 2nd ed.: IUPAC Quantities, Units and Symbols in Physical Chemistry. Second Edition, Blackwell Scientific Publications, Oxford, 1993." ,
                            "PAC, 1994, 66, 533 (Standard quantities in chemical thermodynamics. Fugacities, activities and equilibrium constants for pure and mixed phases (IUPAC Recommendations 1994))" ;
                 skos:changeNote "2024-09-12 Added license and rights fields." ;
                 skos:definition "For a mixture of gases the contribution by each constituent is called the partial pressure p_i = x_i p, where x_i is the amount fraction of the ith constituent and p is the total pressure. [IUPAC]" ;
                 skos:prefLabel "partial pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000003
af-x:AFX_0000003 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000002 ;
                 skos:altLabel "carbon dioxide partial pressure" ,
                               "partial pressure CO2" ;
                 skos:definition "The partial carbon dioxide pressure. [Allotrope]" ;
                 skos:prefLabel "pCO2" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000004
af-x:AFX_0000004 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000002 ;
                 skos:altLabel "oxigen partial pressure" ,
                               "partial pressure O2" ;
                 skos:definition "The pO2 (partial pressure of oxygen) reflects the amount of oxygen gas dissolved in a solution. [Allotrope]" ;
                 skos:prefLabel "pO2" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000005
af-x:AFX_0000005 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 skos:definition "The gas pressure of the collision gas used for collisional excitation. [PSI-MS]" ;
                 skos:prefLabel "collision gas pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000006
af-x:AFX_0000006 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 skos:definition "The pressure of the filtrate. [Allotrope]" ;
                 skos:prefLabel "filtrate pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000007
af-x:AFX_0000007 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 skos:definition "The pressure that is applied during injection. [Allotrope]" ;
                 skos:prefLabel "injection pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000009
af-x:AFX_0000009 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0001155 ;
                 skos:changeNote "2015-11-25 Added concept [Allotrope]" ;
                 skos:definition "The pressure control mode determines the regulation of the partial pressure. [Allotrope]" ;
                 skos:prefLabel "pressure control mode" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000010
af-x:AFX_0000010 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0001686 ;
                 skos:changeNote "2015-11-25 Added concept [Allotrope]" ,
                                 "2016-04-25 move under uses solvent [Allotrope]" ;
                 skos:definition "The pressure controlled solvent is the solvent that is pressurized to a certain partial pressure. [Allotrope]" ;
                 skos:prefLabel "pressure controlled solvent" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000011
af-x:AFX_0000011 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 skos:definition "The pressure of the retentate. [Allotrope]" ;
                 skos:prefLabel "retentate pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000012
af-x:AFX_0000012 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000001 ;
                 skos:altLabel "rinsing pressure" ;
                 skos:definition "The pressure applied during rinsing. [Allotrope]" ;
                 skos:prefLabel "washing pressure" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000013
af-x:AFX_0000013 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000003 ;
                 skos:altLabel "PCO2" ,
                               "PaCO2" ;
                 skos:definition "The measurement of pH, the negative logarithm to base ten of the hydrogen ion activity in a solution. Operationally the pH of a solution X, pH(X), is measured relative to that of a standard reference solution, pH(S), and defined as pH(X) = pH(S) - (E(X) - E(S))/(RT/F)ln 10, where E(X) and E(S) are the electromotive forces measured in cells containing the solution X and the reference solution respectively. [CMO]" ;
                 skos:prefLabel "potential hydrogen" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000014
af-x:AFX_0000014 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000003 ;
                 skos:altLabel "pCO2" ;
                 skos:definition "The force per unit area attributable to the carbon dioxide, a colorless, odorless, incombustible gas formed during respiration and composed of one atom of carbon and two atoms of oxygen, dissolved in a specified volume of blood, defined as the force per unit area exerted by carbon dioxide in a mixture of gases in equilibrium with such a sample of blood at the same temperature. [CMO]" ;
                 skos:prefLabel "arterial carbon dioxide partial pressure (pCO2)" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000015
af-x:AFX_0000015 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000003 ;
                 skos:altLabel "carbond dioxide partial pressure at temperature" ,
                               "partial pressure CO2 at temperature" ;
                 skos:definition "Partial pressure of carbon dioxide at a given temperature. [Allotrope]" ;
                 skos:example "Nova Flex pCO2 at temp" ;
                 skos:prefLabel "pCO2 at temperature" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000016
af-x:AFX_0000016 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000004 ;
                 skos:altLabel "PaO2" ,
                               "partial pressure of blood oxygen (pO2)" ;
                 skos:definition "The force per unit area attributable to the oxygen, the chemical element with atomic number 8 which is the essential agent in the respiration of plants and animals, dissolved in a specified volume of blood, defined as the force per unit area exerted by oxygen in a mixture of gases in equilibrium with such a sample of blood at the same temperature. [CMO]" ;
                 skos:prefLabel "partial pressure of blood oxygen (pO2)" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000017
af-x:AFX_0000017 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000004 ;
                 skos:altLabel "oxigen partial pressure at temperature" ,
                               "partial pressure O2 at temperature" ;
                 skos:definition "Partial pressure of oxygene at a given temperature. [Allotrope]" ;
                 skos:example "Nova Flex pO2 at temp" ;
                 skos:prefLabel "pO2 at temperature" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000018
af-x:AFX_0000018 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000245 ;
                 skos:altLabel "RH" ;
                 skos:definition "Relative humidity (abbreviated RH) is the ratio of the partial pressure of water vapor to the equilibrium vapor pressure of water at the same temperature. Relative humidity depends on temperature and the pressure of the system of interest. [Wikipedia]" ;
                 skos:prefLabel "relative humidity" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000019
af-x:AFX_0000019 rdf:type owl:ObjectProperty ;
                 rdfs:range af-r:AFR_0000304 ;
                 skos:definition "This property relates a certified thing such as a material or a process with its certificate. [Allotrope]" ;
                 skos:prefLabel "certificate" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000020
af-x:AFX_0000020 rdf:type owl:ObjectProperty ;
                 skos:definition "In chemistry, concentration is the abundance of a constituent divided by the total volume of a mixture. Several types of mathematical description can be distinguished: mass concentration, molar concentration, number concentration, and volume concentration. [Wikipedia]" ;
                 skos:example "0.100 mol/L NaOH" ,
                              "event concentration expressed as events per unit of volume in flow cytometry" ;
                 skos:prefLabel "concentration" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000021
af-x:AFX_0000021 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0001153 ;
                 skos:definition "A crystal, crystalline phase is a solid material whose constituents, such as atoms, molecules or ions,are arranged in a highly ordered microscopic structure, forming a crystal lattice that extends in all directions. [Wikipedia]" ;
                 skos:example "Identifying the crystalline phases is the aim of a x-ray powder diffraction measurement." ;
                 skos:prefLabel "crystalline phase" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000024
af-x:AFX_0000024 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0002690 ;
                 skos:definition "The extend of something from end to end. [Allotrope]" ;
                 skos:prefLabel "length" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000025
af-x:AFX_0000025 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000024 ;
                 skos:altLabel "length of diameter" ;
                 skos:definition "For any physical object, the diameter of the object is the same as the diameter of its convex hull that is the supremum of the set of all distances between pairs of points in the object. [Wikipedia]" ;
                 skos:prefLabel "diameter" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000026
af-x:AFX_0000026 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000024 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "PAC, 1985, 57, 105 (Names, symbols, definitions and units of quantities in optical spectroscopy (Recommendations 1984))" ,
                            "PAC, 1996, 68, 2223 (Glossary of terms used in photochemistry (IUPAC Recommendations 1996))" ,
                            "PAC, 1996, 68, 957 (Glossary of terms in quantities and units in Clinical Chemistry (IUPAC-IFCC Recommendations 1996))" ;
                 skos:definition "Distance in the direction of propagation of a periodic wave between two successive points where at a given time the phase is the same. [IUPAC]" ;
                 skos:prefLabel "wavelength" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000027
af-x:AFX_0000027 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000026 ;
                 skos:definition "The wavelength of the electromagnetic spectrum used in the detector. [Allotrope]" ;
                 skos:example "Absorption wavelength in UV detectors." ;
                 skos:prefLabel "detection wavelength" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000028
af-x:AFX_0000028 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000024 ;
                 skos:definition "The path length is the length of the path that the light travels through a sample. [Allotrope]" ;
                 skos:prefLabel "path length" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000029
af-x:AFX_0000029 rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf af-x:AFX_0000025 ;
                 rdfs:range af-c:AFC_0000021 ;
                 skos:definition "The allowed cell diameter defines the range of diameters for which a particle is counted as a cell. [Allotrope]" ;
                 skos:prefLabel "allowed cell diameter" .


###  http://purl.allotrope.org/ontologies/property#AFX_0000030
af-x:AFX_0000030 rdf:type owl:ObjectProperty ;
