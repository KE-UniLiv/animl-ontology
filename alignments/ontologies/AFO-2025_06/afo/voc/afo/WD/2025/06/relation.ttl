###  http://purl.allotrope.org/ontologies/property#AFX_0000326
af-x:AFX_0000326 rdf:type owl:AnnotationProperty ;
                 skos:definition "The measured property of the result. [Allotrope]" ;
                 skos:example "mass of a sample" ;
                 skos:prefLabel "measured property" ;
                 rdfs:subPropertyOf af-x:AFX_0001144 .


###  http://purl.allotrope.org/ontologies/property#AFX_0000391
af-x:AFX_0000391 rdf:type owl:AnnotationProperty ;
                 skos:altLabel "qualifier" ;
                 skos:changeNote "2024-09-12 Added [Allotrope] attribution." ;
                 skos:definition "A qualifier on a property that restricts it to a certain interpretation or aspect that is not a restriction on a class or role. [Allotrope]" ;
                 skos:example "minimum, maximum, limit, average, total" ;
                 skos:prefLabel "has qualifier" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:range af-c:AFC_0000109 .


###  http://purl.allotrope.org/ontologies/property#AFX_0000487
af-x:AFX_0000487 rdf:type owl:AnnotationProperty ;
                 skos:definition "The calculated property of the result. [Allotrope]" ;
                 skos:example "calculated blood gas analysis result for a given temperature." ;
                 skos:prefLabel "calculated property" ;
                 rdfs:subPropertyOf af-x:AFX_0001144 .


###  http://purl.allotrope.org/ontologies/property#AFX_0001140
af-x:AFX_0001140 rdf:type owl:AnnotationProperty ;
                 skos:altLabel "role class" ;
                 skos:changeNote "2016-01-07 add concept to describe property coordinations [Allotrope]" ,
                                 "2016-04-06 remove domain restriction to allow annotation directly on property" ;
                 skos:definition "A relation between a bearer and a role that is a function assumed in a particular situation. [Allotrope]" ;
                 skos:example """\"pump A\" is a class of pumps that has the role \"with identier A\".
				 The role can be modeled as an OWL value restriction on the property dct:identifier.""" ;
                 skos:prefLabel "has role class" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:subPropertyOf af-x:AFX_0001166 .


###  http://purl.allotrope.org/ontologies/property#AFX_0001145
af-x:AFX_0001145 rdf:type owl:AnnotationProperty ;
                 skos:changeNote "2024-09-12 Added [Allotrope] attribution. Fixed typo in definition." ;
                 skos:definition "An annotation property that is used to mark that an object or data property is only defined for grouping purpose within the taxonomy. [Allotrope]" ;
                 skos:prefLabel "is abstract" .


###  http://purl.allotrope.org/ontologies/property#AFX_0001166
af-x:AFX_0001166 rdf:type owl:AnnotationProperty ;
                 skos:changeNote "2016-01-25 add concept to describe property coordination [Allotrope]" ;
                 skos:definition "A relation to a class of individuals. [Allotrope]" ;
                 skos:prefLabel "has class" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:range rdfs:Class .


###  http://purl.allotrope.org/ontologies/property#AFX_0001167
af-x:AFX_0001167 rdf:type owl:AnnotationProperty ;
                 skos:altLabel "primary class" ;
                 skos:changeNote "2016-01-25 add concept for property coordination [Allotrope]" ,
                                 "2016-04-06 remove domain restriction to allow annotation directly on property" ,
                                 "2024-09-12 Added [Allotrope] attribution." ;
                 skos:definition "The primary class is the class that captures the main aspect of a thing or the system. [Allotrope]" ;
                 skos:example "In the coordinated class \"water pump\", \"pump\" is the primary class and \"water\" is a shortcut for the role \"pumps water\"." ;
                 skos:prefLabel "has primary class" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:subPropertyOf af-x:AFX_0001166 .


###  http://purl.allotrope.org/ontologies/property#AFX_0001168
af-x:AFX_0001168 rdf:type owl:AnnotationProperty ;
                 dct:source "Pure Appl. Chem., Vol. 83, No. 10, pp. 1873–1935, 2011" ;
                 skos:altLabel "analyte class" ,
                               "component class" ,
                               "has component class" ;
                 skos:changeNote "2016-01-25 add concept for property coordination [Allotrope]" ;
                 skos:definition "A relation to a class of entities that is part of a primary class that is measured or described by a property. [Allotrope]" ;
                 skos:example "In the coordinated property blood ethanol mass concentration, blood is the primary class (the system) ethanol is the analyte class and mass concentration is the base property. [Allotrope]" ;
                 skos:prefLabel "has analyte class" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:subPropertyOf af-x:AFX_0001140 .


###  http://purl.allotrope.org/ontologies/property#AFX_0001181
af-x:AFX_0001181 rdf:type owl:AnnotationProperty ;
                 skos:altLabel "base property" ;
                 skos:changeNote "2016-04-06 remove domain restriction to allow annotation directly on property" ,
                                 "2024-09-12 Add missing [Allotrope] attribution." ;
                 skos:definition "The property that captures the property of a coordination (n-ary relation). The coordinated property of a coordination is a subproperty of the base property. [Allotrope]" ;
                 skos:prefLabel "has base property" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:subPropertyOf af-x:AFX_0001144 .


###  http://purl.allotrope.org/ontologies/property#AFX_0001182
af-x:AFX_0001182 rdf:type owl:AnnotationProperty ;
                 skos:altLabel "coordination" ;
                 skos:changeNote "2024-09-12 Add missing [Allotrope] attribution." ;
                 skos:definition "A coordinated property is a property that represents several aspects of a base property it is derived from. The property coordination this annotation property points to, describes these aspects. [Allotrope]" ;
                 skos:example "\"device id\" is a coordinated property of the base property \"id\" with the class \"device\"." ;
                 skos:prefLabel "has coordination" ;
                 skos:scopeNote "property coordination" ;
                 rdfs:range af-c:AFC_0000108 ;
                 rdfs:domain rdf:Property .


###  http://purl.allotrope.org/ontologies/property#AFX_0001549
af-x:AFX_0001549 rdf:type owl:AnnotationProperty ;
                 skos:definition "A property which is referencing something. [Allotrope]" ;
                 skos:example "reference time" ;
                 skos:prefLabel "reference property" ;
                 rdfs:subPropertyOf af-x:AFX_0001144 .


