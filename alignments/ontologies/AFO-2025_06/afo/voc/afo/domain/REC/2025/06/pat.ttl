###  http://purl.allotrope.org/ontologies/equipment#AFE_0002224
af-e:AFE_0002224 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000528 ,
                                 af-e:AFE_0002226 ;
                 skos:definition "A reactor jacket is a jacket that controls the temperature of a reactor. [Allotrope]" ;
                 skos:prefLabel "reactor jacket" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002053
af-r:AFR_0002053 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0002056 ;
                 skos:altLabel "reaction temperature profile result" ;
                 skos:definition "A reaction temperature profile is a profile that quantifies the temperature of the reaction solution over time in a synthesis. [Allotrope]" ;
                 skos:prefLabel "reaction temperature profile" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002054
af-r:AFR_0002054 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0002056 ;
                 skos:altLabel "jacket temperature profile result" ;
                 skos:definition "A jacket temperature profile is a profile that quantifies the temperature profile of the reactor jacket over time. [Allotrope]" ;
                 skos:prefLabel "jacket temperature profile" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002341
af-r:AFR_0002341 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0000992 ,
                                 <http://purl.org/linked-data/cube#DataSet> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf af-x:AFX_0002737
                                                  ] ;
                                   owl:someValuesFrom af-r:AFR_0002053
                                 ] ;
                 skos:definition "A reaction temperature data cube is a data cube that represents the reaction temperature over time. [Allotrope]" ;
                 skos:prefLabel "reaction temperature data cube" .


###  http://purl.allotrope.org/ontologies/result#AFR_0002342
af-r:AFR_0002342 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0000992 ,
                                 <http://purl.org/linked-data/cube#DataSet> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf af-x:AFX_0002737
                                                  ] ;
                                   owl:someValuesFrom af-r:AFR_0002054
                                 ] ;
                 skos:definition "A jacket temperature data cube is a data cube that represents the jacket temperature over time. [Allotrope]" ;
                 skos:prefLabel "jacket temperature data cube" .


###  http://purl.allotrope.org/ontologies/result#AFR_0003045
af-r:AFR_0003045 rdf:type owl:Class ;
                 rdfs:subClassOf iao:_0000310 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0002351 ;
                                   owl:someValuesFrom af-r:AFR_0003046
                                 ] ;
                 skos:definition "An automated reactor aggregate document is a document that aggregates automated reactor documents. [Allotrope]" ;
                 skos:prefLabel "automated reactor aggregate document" .


###  http://purl.allotrope.org/ontologies/result#AFR_0003046
af-r:AFR_0003046 rdf:type owl:Class ;
                 rdfs:subClassOf iao:_0000310 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty iao:_0000136 ;
                                   owl:someValuesFrom [ owl:intersectionOf ( af-p:AFP_0002680
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty ro:_0000057 ;
                                                                               owl:someValuesFrom af-e:AFE_0001519
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
                 skos:definition "An automated reactor document is a document that encompasses the information associated with an automated reactor run. [Allotrope]" ;
                 skos:prefLabel "automated reactor document" .


