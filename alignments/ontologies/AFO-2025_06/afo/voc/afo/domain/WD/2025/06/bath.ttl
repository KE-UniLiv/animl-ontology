#################################################################
#    Classes
#################################################################

###  http://purl.allotrope.org/ontologies/equipment#AFE_0001122
af-e:AFE_0001122 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 dct:source "https://en.wikipedia.org/wiki/Laboratory_water_bath#Circulating_Water_Baths" ;
                 skos:altLabel "shaker bath" ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "This type of baths has extra control for shaking, which moves liquids around. [Wikipedia]" ;
                 skos:prefLabel "shaking bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001123
af-e:AFE_0001123 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A bath used for ultrasonic cleaning. [Allotrope]" ;
                 skos:prefLabel "ultrasonic bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001124
af-e:AFE_0001124 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A water bath is a heated bath using water as bath fluid. It is used for temperatures up to 100 °C. [Allotrope]" ;
                 skos:prefLabel "water bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001125
af-e:AFE_0001125 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 dct:source "Gelosa, D. & Sliepcevich, A., Fundamentals of Chemistry, Vol. 1" ,
                            "https://en.wikipedia.org/wiki/Oil_bath" ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "An oil bath, a type of heated bath, is a laboratory heating device which uses boiling oil as the temperature regulator. Since different oils have different boiling points it is possible to obtain a temperature near to the desired temperature by selecting an oil with a boiling point as close as possible to the desired temperature. [Wikipedia]" ;
                 skos:prefLabel "oil bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001126
af-e:AFE_0001126 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:altLabel "circulated bath" ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A bath that continuously moves the fluid. [Allotrope]" ;
                 skos:prefLabel "circulating bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001129
af-e:AFE_0001129 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A heated bath is used in the laboratory to allow a chemical reaction to occur at an elevated temperature. The heated bath is a fluid placed in an open (metal) pot. Water and silicone oil are the most commonly used fluids. A water bath is used for temperatures up to 100 °C. An oil bath is employed for temperatures over 100 °C. [Wikipedia]" ;
                 skos:prefLabel "heated bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001130
af-e:AFE_0001130 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A sand bath is a common piece of laboratory equipment made from a container filled with heated sand. It is used to provide even heating for another container, most often during a chemical reaction. [Wikipedia]" ;
                 skos:prefLabel "sand bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001132
af-e:AFE_0001132 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "This type of bath relies primarily on convection instead of the liquid being uniformly heated. Therefore, it is less accurate in terms of temperature control. In addition, there are add-ons that provide stirring to non-circulating baths to create more uniform heat transfer. [Wikipedia, Allotrope]" ;
                 skos:prefLabel "non-circulating bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001133
af-e:AFE_0001133 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001122 ,
                                 af-e:AFE_0001124 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A combination of a shaker with a water bath. [Allotrope]" ;
                 skos:prefLabel "shaking water bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001134
af-e:AFE_0001134 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001124 ,
                                 af-e:AFE_0001126 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A water bath that is circulated. [Allotrope]" ;
                 skos:prefLabel "circulating water bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001135
af-e:AFE_0001135 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001124 ,
                                 af-e:AFE_0001132 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A water bath that is not circulated. [Allotrope]" ;
                 skos:prefLabel "non-circulating water bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001182
af-e:AFE_0001182 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001121 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A refrigerated bath is used in the laboratory to allow a chemical reaction to occur at a cooler temperature. [Allotrope]" ;
                 skos:prefLabel "refrigerated bath" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001183
af-e:AFE_0001183 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001126 ;
                 skos:changeNote "2015-11-03 add concept [Amgen]" ;
                 skos:definition "A bath where the temperature is controlled by an external circulator. [Allotrope]" ;
                 skos:prefLabel "bath with external circulator" .


