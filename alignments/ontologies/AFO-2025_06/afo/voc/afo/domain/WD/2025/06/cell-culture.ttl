###  http://purl.allotrope.org/ontologies/material#AFM_0000310
af-m:AFM_0000310 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000330 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "basal media" ;
                 skos:definition "A basal medium is a culture medium that provides basic nutrients for the growth of cells. [Allotrope]" ;
                 skos:prefLabel "basal medium" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000330
af-m:AFM_0000330 rdf:type owl:Class ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "medium" ;
                 skos:changeNote "2016-02-02 Added harmonized property restrictions [OSTHUS]" ;
                 skos:definition "A culture medium is a liquid or gel that is used for supporting the growth of cells. [Allotrope]" ;
                 skos:prefLabel "culture medium" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000331
af-m:AFM_0000331 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000330 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A feed medium is a culture medium that supports the growth of cells with feed. [Allotrope]" ;
                 skos:prefLabel "feed medium" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000455
af-m:AFM_0000455 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000275 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0000087 ;
                                   owl:someValuesFrom af-rl:AFRL_0000230
                                 ] ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ;
                 skos:definition "The viability marker is a marker for living cells. [Allotrope]" ;
                 skos:prefLabel "viability marker" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000471
af-m:AFM_0000471 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000275 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0000087 ;
                                   owl:someValuesFrom af-rl:AFRL_0000230
                                 ] ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ;
                 skos:definition "The apoptotsis marker that is used for marking dead cells. [Allotrope]" ;
                 skos:prefLabel "apoptosis marker" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000492
af-m:AFM_0000492 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000275 ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty bfo:_0000051 ;
                                   owl:someValuesFrom af-m:AFM_0000015
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty bfo:_0000051 ;
                                   owl:someValuesFrom af-m:AFM_0000341
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ro:_0000087 ;
                                   owl:someValuesFrom af-rl:AFRL_0000035
                                 ] ;
                 skos:changeNote "2015-11-25 Added concept [Amgen]" ;
                 skos:definition "A cell culture sample is a sample containing media, buffer and cells. [Allotrope]" ;
                 skos:prefLabel "cell culture sample" .


