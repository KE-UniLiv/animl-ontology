#################################################################
#    Classes
#################################################################

###  http://purl.allotrope.org/ontologies/process#AFP_0000427
af-p:AFP_0000427 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002124 ;
                 skos:altLabel "power-compensation DSC" ;
                 skos:definition "The measurement of thermodynamic parameters (e.g. enthalpy) during a chemical or biochemical reaction of both a sample and a reference, by the known variation (step-wise or linear) of one variable, whilst a second is kept constant using a power compensation differential scanning calorimeter. [CHMO]" ;
                 skos:prefLabel "power-compensation differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000595
af-p:AFP_0000595 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002124 ;
                 skos:altLabel "TMDSC" ;
                 skos:definition "The measurement of thermodynamic parameters (e.g. enthalpy) during a chemical or biochemical reaction of both a sample and a reference, by the known variation (step-wise or linear) of temperature whilst a second variable is kept constant. [CHMO]" ;
                 skos:prefLabel "temperature-modulated differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001402
af-p:AFP_0001402 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000595 ;
                 skos:altLabel "DDSCle and a reference, by the known variation (step-wise or linear) of temperature whilst a second variable is kept constant. The power input is in the form of a saw-tooth wave. [CHMO]" ;
                 skos:prefLabel "dynamic differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002132
af-p:AFP_0002132 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000595 ;
                 skos:altLabel "MDSC" ;
                 skos:definition "The measurement of thermodynamic parameters (e.g. enthalpy) during a chemical or biochemical reaction of both a sample and a reference, by the known variation (step-wise or linear) of temperature whilst a second variable is kept constant. The power input is in the form of a sine wave. [CHMO]" ;
                 skos:prefLabel "modulated differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002225
af-p:AFP_0002225 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002124 ;
                 skos:altLabel "heat-flux DSC" ;
                 skos:definition "The measurement of thermodynamic parameters (e.g. enthalpy) during a chemical or biochemical reaction of both a sample and a reference, by the known variation (step-wise or linear) of one variable, whilst a second is kept constant using a heat-flux differential scanning calorimeter. [CHMO]" ;
                 skos:prefLabel "heat-flux differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002650
af-p:AFP_0002650 rdf:type owl:Class ;
                 rdfs:subClassOf af-c:AFC_0000050 ;
                 skos:altLabel "FB mode code" ,
                               "feedback mode code" ;
                 skos:definition "The FB mode code is the code of the feedback mode used for a DSC measurement. [Allotrope]" ;
                 skos:prefLabel "differential scanning calorimetry feedback mode code" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002694
af-p:AFP_0002694 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002124 ;
                 skos:definition "Isothermal differential scanning calorimetry is a type of differential scanning calorimetry where the measurement takes place at constant temperature. [Allotrope]" ;
                 skos:prefLabel "isothermal differential scanning calorimetry" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002697
af-p:AFP_0002697 rdf:type owl:Class ;
                 rdfs:subClassOf af-c:AFC_0000050 ;
                 skos:altLabel "DSC mode code" ;
                 skos:definition "The differential scanning calorimetry scan mode code encodes the mode of measurement of DSC. [Allotrope]" ;
                 skos:prefLabel "differential scanning calorimetry scan mode code" .


###  http://purl.allotrope.org/ontologies/result#AFR_0000238
af-r:AFR_0000238 rdf:type owl:Class ;
                 rdfs:subClassOf af-r:AFR_0000576 ;
                 skos:altLabel "heat release rate curve" ,
                               "heat-release rate curve" ,
                               "heat-release-rate curves" ;
                 skos:definition "A plot of heat release (W/m2) vs. time obtained during a calorimetry experiment. [CHMO]" ;
                 skos:prefLabel "heat-release-rate curve" .


