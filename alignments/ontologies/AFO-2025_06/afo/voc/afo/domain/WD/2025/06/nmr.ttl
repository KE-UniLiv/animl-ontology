                 skos:prefLabel "wide bore magnet" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000001
af-p:AFP_0000001 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000759 ;
                 skos:definition "A dimensionality reduction technique used in multidimensional NMR experiments. A nD NMR experiment is represented in n-1 dimensions by concerted, simultaneous interrogation of two spectral dimensions. [CHMO]" ;
                 skos:prefLabel "accordion spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000024
af-p:AFP_0000024 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002287 ;
                 skos:altLabel "selective INEPT" ;
                 skos:definition "A heteronuclear NMR experiment in which magnetization arising from 1H nuclei is transferred to covalently bound heteronuclei, thereby enhancing the NMR signal of the heteronuclei. Selective pulses, that only excite or 'select' some of the signals in the spectrum are used in this experiment. [CHMO]" ;
                 skos:prefLabel "selective insensitive nuclei enhancement by polarization transfer" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000038
af-p:AFP_0000038 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001702 ;
                 skos:altLabel "pulse ENDOR" ,
                               "pulse ENDOR spectroscopy" ,
                               "pulsed ENDOR" ,
                               "pulsed ENDOR spectroscopy" ;
                 skos:definition "A type of spectroscopy where a microwave frequency pulse is used to saturate the electron resonance transitions in a sample, and a radio frequency pulse is used excite a nuclear transition. [CHMO]" ;
                 skos:prefLabel "pulsed electron nuclear double resonance spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000062
af-p:AFP_0000062 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001230 ;
                 skos:altLabel "3D COSY-NOESY" ,
                               "3D-COSY-NOESY" ,
                               "COSY-NOESY" ;
                 skos:definition "A type of homonuclear three-dimensional NMR experiment in which a second frequency dimension is employed to disperse the signals and reveal correlations between nuclei which are J-coupled (usually <= 3 bonds apart). These signals are then correlated to nuclei that are spatially proximate within the molecule (<= 6 Angstrom apart). [CHMO]" ;
                 skos:prefLabel "correlation spectroscopy-nuclear Overhauser enhancement spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000074
af-p:AFP_0000074 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000681 ;
                 skos:definition "The nuclei being irradiated with an RF pulse is a different nuclei or the same nuclei and a different isotope that the nuclei being observed to relax back to equilibrium for generation of the NMR spectrum. [Allotrope]" ;
                 skos:prefLabel "heteronuclear decoupling" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000086
af-p:AFP_0000086 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001888 ;
                 skos:altLabel "BIRD" ,
                               "bilinear rotation decoupling" ;
                 skos:definition "A pulse cluster (a spin echo on 1H with inversion of 13C in the centre) used to achieve selective inversion of 12C-bound protons, thus achieving the removal of signals due to 12C-bound nuclei. For the pulse sequence see Chem. Phys. Lett., (1982), 93, pg 504. [CHMO]" ;
                 skos:prefLabel "bilinear rotation decoupling pulse sequence" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000103
af-p:AFP_0000103 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000906 ;
                 skos:altLabel "TPPM" ,
                               "TPPM pulse sequence" ,
                               "TPPM sequence" ,
                               "two pulse phase modulation" ,
                               "two-pulse phase modulation" ;
                 skos:definition "A continuous sequence of pulses used in magic angle spinning NMR to remove the effects of heteronuclear dipolar coupling. For the pulse sequence see J. Chem. Phys., (1996), 103, pg 6951. [CHMO]" ;
                 skos:prefLabel "two pulse phase modulation pulse sequence" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000127
af-p:AFP_0000127 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000305 ;
                 skos:altLabel "PFG-SE NMR" ,
                               "PFGSE" ,
                               "PFGSE-NMR" ,
                               "PGSE" ,
                               "pulsed field gradient (PFG) spin echo" ,
                               "pulsed field gradient spin-echo" ,
                               "pulsed field gradient-spin echo" ,
                               "pulsed field-gradient spin echo" ,
                               "pulsed-field-gradient spin-echo NMR spectroscopy" ;
                 skos:definition "An NMR experiment in which pulsed field gradients are applied to a spin echo sequence to cleanly and precisely excite particular resonances. Additionally, attenuation of signals occurs related to molecular diffusion allowing this technique to be used to determine self-diffusion coefficients. [CHMO]" ;
                 skos:prefLabel "pulsed field-gradient spin echo nuclear magnetic resonance spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000144
af-p:AFP_0000144 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001882 ;
                 skos:altLabel "GHMBC" ,
                               "gHMBC" ,
                               "gradient heteronuclear multiple bond coherence" ,
                               "gradient-selected HMBC" ;
                 skos:definition "A type of heteronuclear NMR spectroscopy where pulsed-field gradients are used and a second frequency dimension is employed to disperse the signals and reveal correlations between protons and nuclei which are 2-3 bonds removed. [CHMO]" ;
                 skos:prefLabel "gradient-selected heteronuclear multiple bond coherence" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000199
af-p:AFP_0000199 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000038 ;
                 skos:altLabel "95Mo ENDOR" ,
                               "95Mo ENDOR spectroscopy" ,
                               "molybdenum-95 ENDOR" ,
                               "molybdenum-95 ENDOR spectroscopy" ;
                 skos:definition "A type of spectroscopy where a microwave frequency pulse is used to saturate the electron resonance transitions in the 95Mo nuclei of a sample, and a radio frequency pulse is used excite a nuclear transition. [CHMO]" ;
                 skos:prefLabel "95Mo electron nuclear double resonance spectroscopy" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000212
af-p:AFP_0000212 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000318 ;
                 skos:altLabel "MAS QCPMG" ,
                               "MAS-QCPMG" ,
                               "QCPMG MAS" ,
                               "QCPMG-MAS" ;
                 skos:definition "An NMR experiment used to enhance the sensitivity of low gyromagnetic ratio nuclei by collapsing the intensity of the 'full' spectrum into discrete 'spikelets'/peaks. The intensities of the peaks vary according to the spin-spin relaxation rate (T2) and the envelope described by these peaks represents the full, static spectrum. The solid sample is spun rapidly at the magic angle (54.74 deg. with respect to the direction of the magnetic field) to reduce line broadening. [CHMO]" ;
                 skos:prefLabel "quadrupole Carr-Purcell-Meiboom-Gill using magic-angle spinning" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000248
af-p:AFP_0000248 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000759 ;
                 skos:altLabel " inversion recovery Fourier transform " ,
                               "IRFT" ,
                               "T1" ,
                               "relaxation time measurement" ;
                 skos:definition "An NMR experiment which measures spin-lattice (T1) relaxation rates by acquiring spectra at different delay times following inversion of the spin-state populations. [CHMO]" ;
