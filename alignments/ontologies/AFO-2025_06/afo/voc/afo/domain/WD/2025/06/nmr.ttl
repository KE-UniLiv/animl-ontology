                 skos:altLabel "HCN" ,
                               "HCN probe" ;
                 skos:definition "A triple channel probe is a type of NMR tube that allows for measurements of three different nuclei at the same time. [Allotrope]" ;
                 skos:prefLabel "triple channel probe" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001646
af-e:AFE_0001646 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000127 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 af-x:AFX_0002808 obi:_0000523 ;
                 skos:altLabel "ssNMR tube" ;
                 skos:definition "A rotor is a type of NMR tube usually made of Zirconia that allows for solid state NMR measurements. It can rotate at high frequencies. [Allotrope]" ;
                 skos:prefLabel "rotor" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001647
af-e:AFE_0001647 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001646 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A thin wall rotor is a type of ssNMR tube with a thin wall diameter. [Allotrope]" ;
                 skos:prefLabel "thin wall rotor" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001649
af-e:AFE_0001649 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001648 ;
                 skos:definition "A rotor cap is a type of cap to seal ssNMR tubes. [Allotrope]" ;
                 skos:example "Vespel cap, Kel-F cap" ;
                 skos:prefLabel "rotor cap" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001650
af-e:AFE_0001650 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001646 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "A thick wall rotor is a type of ssNMR tube with a thick wall diameter. [Allotrope]" ;
                 skos:prefLabel "thick wall rotor" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001652
af-e:AFE_0001652 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000354 ;
                 skos:altLabel "radiofrequency transmitter" ,
                               "transmitter" ,
                               "waveform generator" ;
                 skos:definition "A pulse generator is a device that generates electromagnetic pulses with a certain waveform. [Allotrope]" ;
