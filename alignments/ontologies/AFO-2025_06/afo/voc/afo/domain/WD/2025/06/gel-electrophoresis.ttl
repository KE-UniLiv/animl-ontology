#################################################################
#    Classes
#################################################################

###  http://purl.allotrope.org/ontologies/process#AFP_0000186
af-p:AFP_0000186 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000781 ;
                 skos:altLabel "native-native 2D PAGE" ;
                 skos:definition "A two-dimensional separation method where the separation processes are both native polyacrylamide gel electrophoresis. In native polyacrylamide gel electrophoresis protein molecules move at different speeds, in non-denaturing conditions, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. [CHMO]" ;
                 skos:prefLabel "native-native two-dimensional polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000210
af-p:AFP_0000210 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000454 ;
                 skos:altLabel "IEF-PAGE" ,
                               "isoelectrofocusing PAGE " ;
                 skos:definition "A two-dimensional separation method where one separation process is isoelectric focusing on a gel and the other separation process is polyacrylamide gel electrophoresis. [CHMO]" ;
                 skos:prefLabel "isoelectric focusing polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000269
af-p:AFP_0000269 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002133 ;
                 skos:altLabel "BN-PAGE" ,
                               "blue native PAGE" ,
                               "blue-native PAGE" ;
                 skos:definition "A separation method where protein molecules move at different speeds, in non-denaturing conditions, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. The dye Coomassie blue is used to transfer charge to the protein. [CHMO]" ;
                 skos:prefLabel "blue native polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000419
af-p:AFP_0000419 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001043 ;
                 skos:altLabel "SDS PAGE" ,
                               "SDS-PAGE" ;
                 skos:definition "A separation method where colloidal particles move at different speeds according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied, and the electrophoretic mobility of the analytes, usually proteins is altered by their binding to sodium dodecyl sulfate. [CHMO]" ;
                 skos:prefLabel "sodium dodecyl sulfate polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000454
af-p:AFP_0000454 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001969 ;
                 skos:altLabel "2-DGE" ,
                               "2D gel electrophoresis" ,
                               "2DGE" ;
                 skos:definition "A two-dimensional separation method where the separation processes are both gel electrophoresis. [CHMO]" ;
                 skos:prefLabel "two dimensional gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000643
af-p:AFP_0000643 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001080 ;
                 skos:definition "A separation method where colloidal particles move at different speeds according to their electrophoretic mobilities in a separation medium consisting of agarose gel, across which an electric field is applied. [CHMO]" ;
                 skos:prefLabel "agarose gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000781
af-p:AFP_0000781 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000454 ;
                 skos:altLabel "2D PAGE" ,
                               "two dimensional polyacrylamide gel electrophoresis" ;
                 skos:definition "A two-dimensional separation method where the separation processes are both polyacrylamide gel electrophoresis. [CHMO]" ;
                 skos:prefLabel "two-dimensional polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000928
af-p:AFP_0000928 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002332 ;
                 skos:altLabel "CN-PAGE" ,
                               "clear native PAGE" ,
                               "clear-native PAGE" ,
                               "colorless native PAGE " ,
                               "colorless-native PAGE " ;
                 skos:definition "A separation method where metalloprotein molecules move at different speeds, in a buffer with pH 10, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. This method does not use a dye to transfer charge to the protein molecules and the concentration of metal atoms from the metallproteins is determined by ICP-MS. [CHMO]" ;
                 skos:prefLabel "quantitative preparative native continuous polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0000932
af-p:AFP_0000932 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000454 ;
                 skos:altLabel "DIGE" ,
                               "differential in gel electrophoresis" ,
                               "differential in-gel electrophoresis" ;
                 skos:definition "A two dimensional gel electrophoresis method where protein analytes have been previously labelled with different fluorescent dyes and different analytes are compared. [CHMO]" ;
                 skos:prefLabel "difference gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001043
af-p:AFP_0001043 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001080 ;
                 skos:altLabel "PAGE" ;
                 skos:definition "A separation method where colloidal particles move at different speeds according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. [CHMO]" ;
                 skos:prefLabel "polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001081
af-p:AFP_0001081 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0000781 ;
                 skos:altLabel "2D blue native polyacrylamide gel electrophoresis" ,
                               "blue native 2D PAGE" ,
                               "two-dimensional blue native polyacrylamide gel electrophoresis" ;
                 skos:definition "A two-dimensional separation method where the separation processes are both blue native polyacrylamide gel electrophoresis. In native polyacrylamide gel electrophoresis protein molecules move at different speeds, in non-denaturing conditions, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. The dye Coomassie blue is used to transfer charge to the protein. [CHMO]" ;
                 skos:prefLabel "blue native two-dimensional polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001298
af-p:AFP_0001298 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001080 ;
                 skos:altLabel "DGGE" ;
                 skos:definition "A separation method where colloidal particles consisting of polynucleotides move at different speeds according to their charge and frictional forces in a separation medium consisting of a gel that contains a denaturing agent that causes double-stranded DNA to unzip, across which an electric field is applied. [CHMO]" ;
                 skos:prefLabel "denaturing gradient gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0001548
af-p:AFP_0001548 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001080 ;
                 skos:altLabel "TGGE" ;
                 skos:definition "A separation method where colloidal particles consisting of polynucleotides move at different speeds according to their charge and frictional forces in a separation medium consisting of a gel, across which an electric field is applied and across which there is a gradient of temperatures that cause double-stranded DNA to unzip. [CHMO]" ;
                 skos:prefLabel "temperature gradient gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002081
af-p:AFP_0002081 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001080 ,
                                 af-p:AFP_0002297 ;
                 skos:altLabel "PFGE" ,
                               "pulsed field gel electrophoresis" ;
                 skos:definition "A separation method where colloidal particles move at different speeds according to their electrophoretic mobilities in a separation medium consisting of gel, across which a pulsed electric field is applied (one in which the voltage direction is periodically switched among three directions; one that runs through the central axis of the gel and two that run at an angle of 120 deg. either side). [CHMO]" ;
                 skos:prefLabel "pulsed-field gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002133
af-p:AFP_0002133 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0001043 ;
                 skos:altLabel "N-PAGE" ,
                               "NPAGE" ,
                               "native PAGE" ;
                 skos:definition "A separation method where protein molecules move at different speeds, in non-denaturing conditions, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. [CHMO]" ;
                 skos:prefLabel "native polyacrylamide gel electrophoresis" .


###  http://purl.allotrope.org/ontologies/process#AFP_0002332
af-p:AFP_0002332 rdf:type owl:Class ;
                 rdfs:subClassOf af-p:AFP_0002133 ;
                 skos:altLabel "CN-PAGE" ,
                               "clear native PAGE" ,
                               "clear-native PAGE" ,
                               "colorless native PAGE " ,
                               "colorless-native PAGE " ;
                 skos:definition "A separation method where protein molecules move at different speeds, in non-denaturing conditions, according to their charge and frictional forces in a separation medium consisting of polyacrylamide gel, across which an electric field is applied. This method does not use a dye to transfer charge to the protein molecules and is used to separate acidic water-soluble and membrane proteins. [CHMO]" ;
                 skos:prefLabel "clear native polyacrylamide gel electrophoresis" .


