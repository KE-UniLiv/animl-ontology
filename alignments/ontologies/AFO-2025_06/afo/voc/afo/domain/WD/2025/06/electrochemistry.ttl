                 skos:changeNote "2016-02-02 add harmonized property restrictions [OSTHUS]" ;
                 skos:definition "An electrode that serves merely to carry the current flowing through an electrochemical cell. [CHMO]" ;
                 skos:prefLabel "auxiliary electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000036
af-e:AFE_0000036 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000028 ;
                 skos:altLabel "Hydrogen gas electrode" ,
                               "NHE" ,
                               "SHE" ,
                               "hydrogen gas electrode" ,
