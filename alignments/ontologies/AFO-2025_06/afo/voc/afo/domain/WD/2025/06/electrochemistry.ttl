                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "An electrode whose response indicates the change of some analytical variable. [CHMO]" ;
                 skos:prefLabel "interrogating electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000664
af-e:AFE_0000664 rdf:type owl:Class ;
