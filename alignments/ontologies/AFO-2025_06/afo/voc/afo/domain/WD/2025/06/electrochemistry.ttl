                 skos:prefLabel "dissolved oxygen sensor" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000766
af-e:AFE_0000766 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000422 ;
                 skos:altLabel "ISE" ;
                 skos:definition "An electrode which responds to the activity of a primary ion in presence of various other (interfering) ions in the sample solution. Ion-selective electrodes contain a selective membrane which contains fixed or mobile sites that interact with ions in solution. [CHMO]" ;
                 skos:prefLabel "ion-selective electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000770
af-e:AFE_0000770 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000028 ;
                 skos:definition "A ceramic type reference electrode has a ceramic junction as connection to the surrounding solution. [Allotrope]" ;
                 skos:prefLabel "ceramic type reference electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000820
af-e:AFE_0000820 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000028 ;
                 skos:altLabel "pseudo-reference electrodes" ,
                               "pseudoreference electrode" ;
                 skos:definition "An electrode through which no appreciable current is allowed to flow and is used to observe or control the potential at a working electrode. A pseudo-reference electrode, such as a Ag wire, fulfils the role of a reference electrode but the electrochemical processes allowing it to do so are not well understood, in contrast to a true reference electrode. [CHMO]" ;
                 skos:prefLabel "pseudo-reference electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000870
af-e:AFE_0000870 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000028 ;
                 skos:definition "A pinhole type reference electrode has a small hole as connection to the surrounding solution. [Allotrope]" ;
                 skos:prefLabel "pinhole type reference electrode" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000073
af-m:AFM_0000073 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0002099 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "A boundary between two electrolyte solutions of different composition, across which arises a potential difference, called the liquid junction potential. In an operational pH cell, the junction is between the test solution and the filling solution or bridge solution of the reference electrode. [CHMO]" ;
                 skos:prefLabel "liquid junction" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000086
af-m:AFM_0000086 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000012 ;
                 skos:altLabel "electrolyte" ,
                               "ionic solution" ,
                               "salt solution" ;
                 skos:definition "An electrolyte solution is the solution of a substance that ionizes when dissolved in suitable ionizing solvents such as water. [Wikipedia]" ;
                 skos:example "sodium chloride in water" ;
                 skos:prefLabel "electrolyte solution" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000093
af-m:AFM_0000093 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000086 ;
                 owl:disjointWith af-m:AFM_0000279 ;
                 skos:definition "An aqueous electrolyte solution is one that contains some amount of water. [Allotrope]" ;
                 skos:prefLabel "aqueous electrolyte solution" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000097
af-m:AFM_0000097 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000012 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "PAC, 1985, 57, 531. (Definition of pH scales, standard reference values, measurement of pH and related terminology (Recommendations 1984)) on page 541" ;
                 skos:altLabel "salt-bridge solution" ;
                 skos:changeNote "2024-09-12 Changed [IUPAC Recommendations 2002] attribution to [IUPAC] and added license, rights, and source fields." ;
                 skos:definition "Solution of high concentration of inert salt, preferably comprising cations and anions of equal mobility, optionally interposed between the reference electrode filling and both the unknown and standard solution, when the test solution and filling solution are chemically incompatible. This procedure introduces into the cell a second liquid junction formed, usually, in a similar way to the first. [IUPAC]" ;
                 skos:prefLabel "bridge solution" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000150
af-m:AFM_0000150 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000012 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "PAC, 1985, 57, 531. (Definition of pH scales, standard reference values, measurement of pH and related terminology (Recommendations 1984)) on page 541" ;
                 skos:changeNote "2024-09-12 Changed [IUPAC Recommendations 2002] attribution to [IUPAC] and added license, rights, and source fields." ;
                 skos:definition "Solution containing the anion to which the reference electrode of Cells IV and V is reversible, e.g., chloride for silverâ€“silver chloride electrode. In the absence of a bridge solution (q.v.), a high concentration of filling solution comprising almost equitransferent cations and anions is employed as a means of maintaining the LJP small and approximately constant on substitution of unknown solution for standard solution(s). [IUPAC]" ;
                 skos:prefLabel "filling solution" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000170
af-m:AFM_0000170 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000086 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "An electrolyte that is used for ion selective electrodes. [Allotrope]" ;
                 skos:prefLabel "electrolyte for ISE" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000279
af-m:AFM_0000279 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000086 ;
                 skos:definition "A nonaqueous electrolyte solution is free from water. [Allotrope]" ;
                 skos:prefLabel "nonaqueous electrolyte solution" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000343
af-m:AFM_0000343 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000086 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:definition "An electrolyte that is used for gas sensing electrodes. [Allotrope]" ;
                 skos:prefLabel "electrolyte for GSE" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000360
af-m:AFM_0000360 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000015 ;
                 skos:altLabel "IEF anode buffer" ,
                               "IEF anodic buffer" ,
                               "IEF cathode buffer" ,
                               "anodic buffer" ,
                               "isoelectric focusing anode buffer" ,
