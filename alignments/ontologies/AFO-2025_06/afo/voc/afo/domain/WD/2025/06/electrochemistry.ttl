                 skos:altLabel "Ag-AgCl electrode" ,
                               "Ag-AgCl reference electrode" ,
                               "Ag/AgCl" ,
                               "Ag/AgCl electrode" ,
                               "Ag/AgCl reference electrode" ,
                               "silver chloride electrode " ,
                               "silver-silver chloride electrode" ;
                 skos:definition "A reference electrode where the electrochemical reaction is between silver metal and silver(I) chloride. [CHMO]" ;
                 skos:prefLabel "silver/silver chloride reference electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000083
af-e:AFE_0000083 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000721 ;
                 skos:altLabel "RDE" ,
                               "rotating disk electrode" ;
                 skos:definition "A type of working electrode which usually consists of a Pt disc (diameter 1.0 mm) sealed in a Teflon carrier which rotates a high speed during the experiment. Rotating-disk electrodes are used to investigate reaction mechanisms. [CHMO]" ;
                 skos:prefLabel "rotating-disk electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000177
af-e:AFE_0000177 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000422 ;
                 af-x:AFX_0002798 "true"^^xsd:boolean ;
                 skos:altLabel "dissolved gas sensor" ;
                 skos:changeNote "2015-12-14 Improved definition [Amgen]" ,
                                 "2016-02-02 add harmonized property restrictions [OSTHUS]" ;
                 skos:definition "A gas sensing electrode (GSE) measures dissolved oxygen and carbon dioxide concentrations in an aqueous sample in the form of partial pressure. [Allotrope]" ;
                 skos:prefLabel "gas sensing electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000281
af-e:AFE_0000281 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000115 ;
                 skos:definition "The Standard hydrogen cell forms the basis of the thermodynamic scale of oxidation-reduction potentials. Its absolute electrode potential is estimated to be 0.41 ± 0.02 V at 25 °C, but to form a basis for comparison with all other electrode reactions, hydrogen's standard electrode potential (E0) is declared to be zero at all temperatures. Potentials of any other electrodes are compared with that of the standard hydrogen cell at the same temperature. [Wikipedia]" ;
                 skos:prefLabel "standard hydrogen cell" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000289
af-e:AFE_0000289 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000335 ;
                 skos:altLabel "HMDE" ,
                               "SMDE" ,
                               "static drop mercury electrode" ;
                 skos:definition "An electrode formed of a partial mercury drop of controlled geometry and surface area at the end of a capillary. [CHMO]" ;
                 skos:prefLabel "hanging mercury drop electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000293
af-e:AFE_0000293 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001785 ;
                 dct:license <http://purl.allotrope.org/voc/iupac-license> ;
                 dct:rights <http://purl.allotrope.org/voc/iupac-copyright> ;
                 dct:source "PAC, 1985, 57, 531. (Definition of pH scales, standard reference values, measurement of pH and related terminology (Recommendations 1984)) on page 541" ;
                 skos:altLabel "liquid junction" ;
                 skos:changeNote "2015-11-26 subclass under barrier [OSTHUS]" ,
                                 "2016-02-02 add harmonized property restrictions [OSTHUS]" ,
                                 "2024-09-12 Changed [IUPAC Recommendations 2002] attribution to [IUPAC] and added license, rights, and source fields." ;
                 skos:definition "Any junction between two electrolyte solutions of different composition. Across such a junction there arises a potential difference, called the liquid junction potential. In Cells II, IV, and V, the junction is between the pH standard or unknown solution and the filling solution, or the bridge solution (q.v.), of the reference electrode. [IUPAC]" ;
                 skos:prefLabel "electrode junction" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000335
af-e:AFE_0000335 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000422 ;
                 skos:altLabel "DME" ;
                 skos:definition "An electrode formed by a sequence of mercury drops falling from a small aperture. [CHMO]" ;
                 skos:prefLabel "dropping mercury electrode" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0000361
