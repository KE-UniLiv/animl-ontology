###  http://purl.allotrope.org/ontologies/equipment#AFE_0001338
af-e:AFE_0001338 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000074 ;
                 skos:altLabel "HVAC" ,
                               "heating, ventilating, and air conditioning" ,
                               "heating, ventilating, and air conditioning system" ;
                 skos:changeNote "2024-09-12 Changed [OSTHUS] attribution to [Allotrope]." ;
                 skos:definition "A system to maintain a controlled and acceptable thermal comfort and air quality. [Allotrope]" ;
                 skos:prefLabel "HVAC system" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001461
af-e:AFE_0001461 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001462 ;
                 dct:source "WHO TRS 993" ;
                 skos:altLabel "primary container" ;
                 skos:definition "A primary container closure system is a packaging component (for example, a vial or pre-filled syringe) that is in, or may come into, direct contact with the final product dosage form, or components that contribute to the container/closure integrity of the primary packaging material for a sterile product. [WHO TRS 993]" ;
                 skos:prefLabel "primary container closure system" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001462
af-e:AFE_0001462 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0000074 ;
                 skos:altLabel "packaging system" ;
                 skos:definition "A container closure system refers to the sum of packaging components that together contain and protect the dosage form. This includes primary packaging components and secondary packaging components, if the latter are intended to provide additional protection to the drug product. [FDA]" ;
                 skos:prefLabel "container closure system" .


###  http://purl.allotrope.org/ontologies/equipment#AFE_0001463
af-e:AFE_0001463 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0001462 ;
                 dct:source "WHO TRS 993" ;
                 skos:altLabel "secondary container" ,
                               "secondary package" ;
                 skos:definition "A secondary container closure system is a packaging component (for example, a carton or tray) that is not, and will not be, in direct contact with the dosage form. [WHO TRS 993]" ;
                 skos:prefLabel "secondary container closure system" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000033
af-m:AFM_0000033 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000091 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "A laboratory atmosphere consisting of gaseous diatomic nitrogen. [CHMO]" ;
                 skos:prefLabel "nitrogen atmosphere" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000091
af-m:AFM_0000091 rdf:type owl:Class ;
                 rdfs:subClassOf af-e:AFE_0002158 ;
                 af-x:AFX_0002796 af-r:AFR_0001223 ;
                 skos:definition "A gaseous environment in which an experiment takes place. This may be unreactive in order to prevent unintended reactions or reactive (for example gaseous hydrogen) to promote reactions. [CHMO]" ;
                 skos:prefLabel "laboratory atmosphere" .


###  http://purl.allotrope.org/ontologies/material#AFM_0000118
af-m:AFM_0000118 rdf:type owl:Class ;
                 rdfs:subClassOf af-m:AFM_0000091 ;
