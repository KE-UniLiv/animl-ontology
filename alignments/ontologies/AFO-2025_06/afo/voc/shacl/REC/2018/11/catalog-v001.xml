<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE catalog[
<!ELEMENT catalog (uri|group)+>
<!ELEMENT group (uri)+>
<!ELEMENT uri EMPTY>
<!ATTLIST catalog
	xmlns CDATA #IMPLIED
	prefer (system|public) #IMPLIED
>
<!ATTLIST uri
	id ID #IMPLIED
	name CDATA #REQUIRED
	uri CDATA #REQUIRED
>
<!ATTLIST group
	id ID #IMPLIED
	prefer (system|public) #IMPLIED
	xml:base CDATA #IMPLIED
>
]>
<catalog prefer="public" xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
	<group id="import" prefer="public" xml:base="">
	</group>
	<group id="external" prefer="public" xml:base="">
		<uri id="id900" name="http://purl.allotrope.org/voc/shacl/REC/2018/11/shacl" uri="shacl.ttl"/>
		<uri id="id901" name="http://www.w3.org/ns/shacl#" uri="shacl.ttl"/>
	</group>
</catalog>
