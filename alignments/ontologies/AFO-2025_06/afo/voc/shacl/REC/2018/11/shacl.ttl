#################################################################
#    Annotations
#################################################################

sh:and rdfs:comment "RDF list of shapes to validate the value nodes against."@en ;
       rdfs:isDefinedBy sh: ;
       rdfs:label "and"@en .


sh:class rdfs:comment "The type that all value nodes must have."@en ;
         rdfs:isDefinedBy sh: ;
         rdfs:label "class"@en .


sh:closed rdfs:comment "If set to true then the shape is closed."@en ;
          rdfs:isDefinedBy sh: ;
          rdfs:label "closed"@en .


sh:datatype rdfs:comment "Specifies an RDF datatype that all value nodes must have."@en ;
            rdfs:isDefinedBy sh: ;
            rdfs:label "datatype"@en .


sh:disjoint rdfs:comment "Specifies a property where the set of values must be disjoint with the value nodes."@en ;
            rdfs:isDefinedBy sh: ;
            rdfs:label "disjoint"@en .


sh:equals rdfs:comment "Specifies a property that must have the same values as the value nodes."@en ;
          rdfs:isDefinedBy sh: ;
          rdfs:label "equals"@en .


sh:flags rdfs:comment "An optional flag to be used with regular expression pattern matching."@en ;
         rdfs:isDefinedBy sh: ;
         rdfs:label "flags"@en .


sh:ignoredProperties rdfs:comment "An optional RDF list of properties that are also permitted in addition to those explicitly enumerated via sh:property/sh:path."@en ;
